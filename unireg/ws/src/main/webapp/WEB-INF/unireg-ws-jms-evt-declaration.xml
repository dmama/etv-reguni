<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="evenementDeclarationSender" class="ch.vd.uniregctb.evenement.di.EvenementDeclarationSenderImpl">
		<property name="esbTemplate" ref="esbJmsTemplate" />
		<property name="esbMessageFactory" ref="esbMessageFactory"/>
		<property name="serviceDestination" value="${extprop.jms.queue.evtDeclaration.output}" />
		<property name="enabled" value="${extprop.jms.queue.evtDeclaration.output.enabled}" />
	</bean>

</beans>
