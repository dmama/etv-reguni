<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bamMessageSender" class="ch.vd.uniregctb.jms.BamMessageSenderImpl">
		<property name="esbTemplate" ref="esbJmsTemplate"/>
		<property name="esbMessageFactory" ref="esbMessageFactory"/>
		<property name="enabled" value="${extprop.jms.bam.enabled}"/>
	</bean>

</beans>
