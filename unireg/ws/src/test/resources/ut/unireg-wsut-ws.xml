<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<bean id="wsEventDataListenerAdapter" class="ch.vd.uniregctb.webservices.common.WebServiceEventListenerAdapter">
		<property name="dataEventService" ref="dataEventService" />
		<property name="tiersDAO" ref="tiersDAO" />
		<property name="listeners">
			<util:list>
				<ref bean="partyService3CacheManager"/>
				<ref bean="partyService4CacheManager"/>
				<ref bean="wsv5CacheEventListener"/>
				<ref bean="wsv6CacheEventListener"/>
			</util:list>
		</property>
	</bean>

	<!-- Beans du web-service Tiers v3 mais sans le end-point jaxws -->

	<bean id="partyService3EndPoint" class="ch.vd.uniregctb.webservices.party3.impl.PartyWebServiceEndPoint">
		<property name="service" ref="partyService3Cache" />
	</bean>

	<bean id="partyService3Impl" class="ch.vd.uniregctb.webservices.party3.impl.PartyWebServiceImpl">
		<property name="adresseService" ref="adresseService" />
		<property name="tiersDAO" ref="tiersDAO" />
		<property name="tiersService" ref="tiersService" />
		<property name="tiersSearcher" ref="globalTiersSearcher" />
		<property name="infraService" ref="serviceInfrastructureService" />
		<property name="situationService" ref="situationFamilleService" />
		<property name="ibanValidator" ref="ibanValidator" />
		<property name="parametreService" ref="parametreAppService" />
		<property name="serviceCivil" ref="serviceCivilService" />
		<property name="serviceOrganisation" ref="serviceOrganisationService" />
		<property name="hibernateTemplate" ref="hibernateTemplate" />
		<property name="transactionManager" ref="transactionManager" />
		<property name="lrService" ref="lrService" />
		<property name="diService" ref="diService" />
		<property name="bamMessageSender" ref="bamMessageSender" />
		<property name="threadPool" ref="wsThreadPoolParty3" />
		<property name="assujettissementService" ref="assujettissementService" />
		<property name="periodeImpositionService" ref="periodeImpositionService" />
		<property name="bouclementService" ref="bouclementService" />
	</bean>

	<bean id="wsThreadPoolParty3" class="org.springframework.scheduling.concurrent.ThreadPoolExecutorFactoryBean">
		<property name="corePoolSize" value="10"/> <!-- nombre minimum de threads -->
		<property name="maxPoolSize" value="20"/> <!-- nombre maximum de threads -->
	</bean>

	<bean id="partyService3Cache" class="ch.vd.uniregctb.webservices.party3.cache.PartyWebServiceCache">
		<property name="target" ref="partyService3Impl" />
		<property name="cacheManager" ref="ehCacheManager" />
		<property name="cacheName" value="webServiceParty3" />
		<property name="uniregCacheManager" ref="uniregCacheManager" />
	</bean>

	<bean id="partyService3CacheManager" class="ch.vd.uniregctb.webservices.party3.cache.PartyWebServiceCacheManager">
		<property name="cache" ref="partyService3Cache" />
	</bean>

	<!-- Beans du web-service Tiers v4 mais sans le end-point jaxws -->

	<bean id="partyService4EndPoint" class="ch.vd.uniregctb.webservices.party4.impl.PartyWebServiceEndPoint">
		<property name="service" ref="partyService4Cache" />
	</bean>

	<bean id="partyService4Impl" class="ch.vd.uniregctb.webservices.party4.impl.PartyWebServiceImpl">
		<property name="adresseService" ref="adresseService" />
		<property name="tiersDAO" ref="tiersDAO" />
		<property name="tiersService" ref="tiersService" />
		<property name="tiersSearcher" ref="globalTiersSearcher" />
		<property name="infraService" ref="serviceInfrastructureService" />
		<property name="situationService" ref="situationFamilleService" />
		<property name="ibanValidator" ref="ibanValidator" />
		<property name="parametreService" ref="parametreAppService" />
		<property name="serviceCivil" ref="serviceCivilService" />
		<property name="serviceOrganisation" ref="serviceOrganisationService" />
		<property name="hibernateTemplate" ref="hibernateTemplate" />
		<property name="transactionManager" ref="transactionManager" />
		<property name="lrService" ref="lrService" />
		<property name="diService" ref="diService" />
		<property name="bamMessageSender" ref="bamMessageSender" />
		<property name="threadPool" ref="wsThreadPoolParty4" />
		<property name="assujettissementService" ref="assujettissementService" />
		<property name="periodeImpositionService" ref="periodeImpositionService" />
		<property name="bouclementService" ref="bouclementService" />
	</bean>

	<bean id="wsThreadPoolParty4" class="org.springframework.scheduling.concurrent.ThreadPoolExecutorFactoryBean">
		<property name="corePoolSize" value="10"/> <!-- nombre minimum de threads -->
		<property name="maxPoolSize" value="20"/> <!-- nombre maximum de threads -->
	</bean>

	<bean id="partyService4Cache" class="ch.vd.uniregctb.webservices.party4.cache.PartyWebServiceCache">
		<property name="target" ref="partyService4Impl" />
		<property name="cacheManager" ref="ehCacheManager" />
		<property name="cacheName" value="webServiceParty4" />
		<property name="uniregCacheManager" ref="uniregCacheManager" />
	</bean>

	<bean id="partyService4CacheManager" class="ch.vd.uniregctb.webservices.party4.cache.PartyWebServiceCacheManager">
		<property name="cache" ref="partyService4Cache" />
	</bean>

	<!-- Beans du web-service v5 mais sans le end-point jaxrs -->

	<bean id="wsThreadPoolv5" class="org.springframework.scheduling.concurrent.ThreadPoolExecutorFactoryBean">
		<property name="corePoolSize" value="10"/> <!-- nombre minimum de threads -->
		<property name="maxPoolSize" value="20"/> <!-- nombre maximum de threads -->
	</bean>

	<bean id="wsv5Access" class="ch.vd.uniregctb.webservices.v5.BusinessWebServiceAccessChecker">
		<property name="securityProvider" ref="securityProviderInterface"/>
		<property name="target" ref="wsv5Cache"/>
	</bean>

	<bean id="wsv5Cache" class="ch.vd.uniregctb.webservices.v5.cache.BusinessWebServiceCache">
		<property name="target" ref="wsv5Business"/>
		<property name="cacheManager" ref="ehCacheManager" />
		<property name="cacheName" value="webService5" />
		<property name="uniregCacheManager" ref="uniregCacheManager" />
		<property name="statsService" ref="statsService" />
		<property name="securityProvider" ref="securityProviderInterface"/>
	</bean>

	<bean id="wsv5CacheEventListener" class="ch.vd.uniregctb.webservices.v5.cache.BusinessWebServiceCacheEventListener">
		<property name="cache" ref="wsv5Cache"/>
	</bean>

	<bean id="wsv5Business" class="ch.vd.uniregctb.webservices.v5.BusinessWebServiceImpl">
		<property name="securityProvider" ref="securityProviderInterface"/>
		<property name="transactionManager" ref="transactionManager"/>
		<property name="tiersService" ref="tiersService"/>
		<property name="tiersDAO" ref="tiersDAO" />
		<property name="diService" ref="diService"/>
		<property name="bamSender" ref="bamMessageSender"/>
		<property name="infraService" ref="serviceInfrastructureService" />
		<property name="lrService" ref="lrService"/>
		<property name="tiersSearcher" ref="globalTiersSearcher" />
		<property name="adresseService" ref="adresseService" />
		<property name="situationService" ref="situationFamilleService" />
		<property name="ibanValidator" ref="ibanValidator" />
		<property name="parametreService" ref="parametreAppService" />
		<property name="serviceCivil" ref="serviceCivilService" />
		<property name="serviceOrganisation" ref="serviceOrganisationService" />
		<property name="hibernateTemplate" ref="hibernateTemplate" />
		<property name="assujettissementService" ref="assujettissementService" />
		<property name="periodeImpositionService" ref="periodeImpositionService" />
		<property name="periodeImpositionImpotSourceService" ref="periodeImpositionImpotSourceService"/>
		<property name="EFactureService" ref="efactureService"/>
		<property name="bouclementService" ref="bouclementService"/>
		<property name="threadPool" ref="wsThreadPoolv5"/>
		<property name="avatarService" ref="avatarService"/>
	</bean>

	<!-- Beans du web-service v6 mais sans le end-point jaxrs -->

	<bean id="wsThreadPoolv6" class="org.springframework.scheduling.concurrent.ThreadPoolExecutorFactoryBean">
		<property name="corePoolSize" value="10"/> <!-- nombre minimum de threads -->
		<property name="maxPoolSize" value="20"/> <!-- nombre maximum de threads -->
	</bean>

	<bean id="wsv6Access" class="ch.vd.uniregctb.webservices.v6.BusinessWebServiceAccessChecker">
		<property name="securityProvider" ref="securityProviderInterface"/>
		<property name="target" ref="wsv6Cache"/>
	</bean>

	<bean id="wsv6Cache" class="ch.vd.uniregctb.webservices.v6.cache.BusinessWebServiceCache">
		<property name="target" ref="wsv6Business"/>
		<property name="cacheManager" ref="ehCacheManager" />
		<property name="cacheName" value="webService6" />
		<property name="uniregCacheManager" ref="uniregCacheManager" />
		<property name="statsService" ref="statsService" />
		<property name="securityProvider" ref="securityProviderInterface"/>
	</bean>

	<bean id="wsv6CacheEventListener" class="ch.vd.uniregctb.webservices.v6.cache.BusinessWebServiceCacheEventListener">
		<property name="cache" ref="wsv6Cache"/>
	</bean>

	<bean id="wsv6Business" class="ch.vd.uniregctb.webservices.v6.BusinessWebServiceImpl">
		<property name="securityProvider" ref="securityProviderInterface"/>
		<property name="transactionManager" ref="transactionManager"/>
		<property name="tiersService" ref="tiersService"/>
		<property name="tiersDAO" ref="tiersDAO" />
		<property name="diService" ref="diService"/>
		<property name="bamSender" ref="bamMessageSender"/>
		<property name="infraService" ref="serviceInfrastructureService" />
		<property name="lrService" ref="lrService"/>
		<property name="tiersSearcher" ref="globalTiersSearcher" />
		<property name="adresseService" ref="adresseService" />
		<property name="situationService" ref="situationFamilleService" />
		<property name="ibanValidator" ref="ibanValidator" />
		<property name="parametreService" ref="parametreAppService" />
		<property name="serviceCivil" ref="serviceCivilService" />
		<property name="serviceOrganisation" ref="serviceOrganisationService" />
		<property name="hibernateTemplate" ref="hibernateTemplate" />
		<property name="assujettissementService" ref="assujettissementService" />
		<property name="periodeImpositionService" ref="periodeImpositionService" />
		<property name="periodeImpositionImpotSourceService" ref="periodeImpositionImpotSourceService"/>
		<property name="EFactureService" ref="efactureService"/>
		<property name="bouclementService" ref="bouclementService"/>
		<property name="threadPool" ref="wsThreadPoolv6"/>
		<property name="avatarService" ref="avatarService"/>
	</bean>

</beans>
