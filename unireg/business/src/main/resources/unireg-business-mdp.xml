<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Gestionnaire d'evenements: le service réceptionnant le message XML et persistant l'événement en base -->
	<bean id="evenementCivilUnitaireMDP" class="ch.vd.uniregctb.evenement.jms.EvenementCivilUnitaireMDPImpl">
		<property name="evenementCivilUnitaireDAO" ref="evenementCivilUnitaireDAO" />
		<property name="evenementCivilRegrouper" ref="evenementCivilRegrouper" />
		<property name="evenementCivilProcessor" ref="evenementCivilProcessor" />
		<property name="transactionManager" ref="transactionManager" />
	</bean>

</beans>
