<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- Bean client interfacesService MOCK -->
	<bean id="serviceInfrastructureService" class="ch.vd.uniregctb.interfaces.service.mock.ProxyServiceInfrastructureService">
		<property name="tiersDAO" ref="tiersDAO"/>
	</bean>

	<!-- Bean client ServiceCivil MOCK -->
	<bean id="serviceCivilServiceMock" class="ch.vd.uniregctb.interfaces.service.mock.ProxyServiceCivil">
		<property name="infraService" ref="serviceInfrastructureService"/>
	</bean>
	<alias alias="serviceCivilService" name="serviceCivilServiceMock"/>

	<!-- Bean client ServiceOrganisation MOCK -->
	<bean id="serviceOrganisationServiceMock" class="ch.vd.uniregctb.interfaces.service.mock.ProxyServiceOrganisation">
		<constructor-arg index="0" ref="serviceInfrastructureService"/>
	</bean>
	<alias alias="serviceOrganisationService" name="serviceOrganisationServiceMock"/>

	<!-- Service UPI -->
	<bean id="serviceUpiMock" class="ch.vd.unireg.interfaces.upi.mock.DefaultMockServiceUpi"/>
	<alias alias="serviceUpiRaw" name="serviceUpiMock"/>

	<!-- Bean client interfacesService MOCK -->
	<bean id="serviceSecuriteServiceHost" class="ch.vd.uniregctb.interfaces.service.mock.ProxyServiceSecuriteService"/>
	<alias alias="serviceSecuriteService" name="serviceSecuriteServiceHost"/>

</beans>
