<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<!-- POM for Norentes -->

	<modelVersion>4.0.0</modelVersion>

	<artifactId>uniregctb-norentes</artifactId>
	<packaging>jar</packaging>

	<parent>
		<groupId>ch.vd.uniregctb</groupId>
		<artifactId>uniregctb</artifactId>
		<!-- BEGIN_LVERSION -->
		<version>4.11.3</version>
		<!-- END_LVERSION -->
		<relativePath>../base/pom.xml</relativePath>
	</parent>

	<name>Unireg - Norentes</name>
	<description>UniregCTB, Le registre unifie de l'Etat de Vaud - NOn REgressioN TESts</description>

	<dependencies>
		<dependency>
			<groupId>${pom.parent.groupId}</groupId>
			<artifactId>uniregctb-business</artifactId>
			<version>${pom.parent.version}</version>
		</dependency>
		<dependency>
			<groupId>${pom.parent.groupId}</groupId>
			<artifactId>uniregctb-testing</artifactId>
			<version>${pom.parent.version}</version>
		</dependency>
		<dependency>
			<groupId>ch.vd.host-interfaces</groupId>
			<artifactId>host-interfaces-client</artifactId>
			<version>${host-interfaces.version-long}</version>
		</dependency>
	    <dependency>
			<groupId>${pom.parent.groupId}</groupId>
			<artifactId>uniregctb-core</artifactId>
			<version>${pom.parent.version}</version>
			<type>test-jar</type>
			<scope>test</scope>
	    </dependency>
	    <dependency>
			<groupId>${pom.parent.groupId}</groupId>
			<artifactId>uniregctb-business</artifactId>
			<version>${pom.parent.version}</version>
			<type>test-jar</type>
			<scope>test</scope>
	    </dependency>
		<dependency>
			<groupId>javax.ejb</groupId>
			<artifactId>ejb</artifactId>
			<version>2.1</version>
		</dependency>
		<dependency>
			<groupId>javax.xml.stream</groupId>
			<artifactId>stax-api</artifactId>
			<version>1.0-2</version>
		</dependency>
	</dependencies>

	<!-- Build plugins and sources -->
	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<includes>
					<include>**/*</include>
				</includes>
			</resource>
		</resources>
		<testResources>
			<testResource>
				<directory>src/test/resources</directory>
				<filtering>true</filtering>
				<includes>
					<include>**/*</include>
				</includes>
			</testResource>
		</testResources>
		<plugins>
		</plugins>
	</build>

</project>
