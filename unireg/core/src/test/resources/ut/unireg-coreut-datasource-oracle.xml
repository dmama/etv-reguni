<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util" xmlns:j="jelly:core"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<bean id="jdbcManagedConnectionFactory" class="ch.vd.uniregctb.datasource.UniregXAMCF">
		<property name="driverType" value="thin" />
		<property name="serverName" value="${testprop.jdbc.server}" />
		<property name="portNumber" value="${testprop.jdbc.port}" />
		<property name="databaseName" value="${testprop.jdbc.instance}" />
		<property name="userName" value="${testprop.jdbc.username}" />
		<property name="password" value="${testprop.jdbc.password}" />
	</bean>

</beans>
