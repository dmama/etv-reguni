<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:util="http://www.springframework.org/schema/util"
		xsi:schemaLocation="http://www.springframework.org/schema/beans
							http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
							http://www.springframework.org/schema/util
							http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<!-- La session factory -->
	<bean id="sessionFactoryHibernateCollections" class="org.springframework.orm.hibernate3.annotation.AnnotationSessionFactoryBean">
		<property name="annotatedClasses">
			<list>
				<value>ch.vd.uniregctb.hibernate.HibernateCollectionsTest$Tiers</value>
				<value>ch.vd.uniregctb.hibernate.HibernateCollectionsTest$Personne</value>
				<value>ch.vd.uniregctb.hibernate.HibernateCollectionsTest$Menage</value>
				<value>ch.vd.uniregctb.hibernate.HibernateCollectionsTest$Rapport</value>
			</list>
		</property>
		<property name="dataSource" ref="dataSource" />
		<property name="hibernateProperties" ref="hibernateProperties" />
	</bean>
</beans>
