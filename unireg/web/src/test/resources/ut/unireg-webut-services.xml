<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<bean id="uniregExternalProperties" class="ch.vd.uniregctb.utils.MockUniregProperties">
	</bean>

	<bean id="evenementFiscalFacade" class="ch.vd.uniregctb.evenement.fiscal.jms.mock.MockEvenementFiscalFacade" >
	</bean>

	<bean id="jmsEvenementCivilTemplateOutput" class="org.easymock.EasyMock"  factory-method="createNiceMock" >
		<constructor-arg>
			<value type="java.lang.Class" >org.springframework.jms.core.JmsOperations</value>
		</constructor-arg>
	</bean>

	<bean id="servletService" class="ch.vd.uniregctb.servlet.MockServletService">
	</bean>

</beans>
