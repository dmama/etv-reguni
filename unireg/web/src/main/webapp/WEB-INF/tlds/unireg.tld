			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>id à associer au lien</description>
		</attribute>
	</tag>

	<tag>
		<name>callstack</name>
		<tag-class>ch.vd.uniregctb.taglibs.JspTagCallstack</tag-class>
		<body-content>JSP</body-content>
		<description>Permet d'afficher une call-stack (cachée par défaut)</description>
		<attribute>
			<name>headerMessage</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>le message d'entête à afficher avant la callstack</description>
		</attribute>
		<attribute>
			<name>exception</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>l'exception dont on veut afficher la callstack</description>
		</attribute>
		<attribute>
			<name>callstack</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>la callstack déjà résolue de l'exception</description>
		</attribute>
	</tag>

	<tag>
		<name>RetourButton</name>
		<tag-class>ch.vd.uniregctb.taglibs.JspTagRetourButton</tag-class>
		<body-content>JSP</body-content>
		<description>Permet de quitter la page d'edition et de retourner à la page de visualisation</description>
		<attribute>
			<name>link</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>lien de retour</description>
		</attribute>
		<attribute>
			<name>text</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>le texte du bouton ('Retour' par défaut)</description>
		</attribute>
		<attribute>
			<name>message</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>le texte à afficher dans la boite de dialogue</description>
		</attribute>
		<attribute>
			<name>checkIfModified</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>s'il faut vérifier l'état du Modifier avant d'afficher le message de confirmation</description>
		</attribute>
	</tag>

	<tag>
		<name>bandeauTiers</name>
		<tag-class>ch.vd.uniregctb.taglibs.JspTagBandeauTiers</tag-class>
		<body-content>JSP</body-content>
		<description>Affiche le bandeau d'un contribuable</description>
		<attribute>
			<name>numero</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>le numéro de contribuable</description>
		</attribute>
		<attribute>
			<name>titre</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>le titre affiché dans le bandeau (défaut='Caractéristiques du tiers')</description>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>la classe Html utilisée par le fieldset principal du bandeau (défaut='information')</description>
		</attribute>
		<attribute>
			<name>showValidation</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>affiche ou non les erreurs de validation (défaut=true)</description>
		</attribute>
		<attribute>
			<name>showEvenementsCivils</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>affiche ou non les événements civils en erreur associé au contribuable (défaut=true)</description>
		</attribute>
		<attribute>
			<name>showLinks</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>affiche ou non les liens vers des pages externes (défaut=true)</description>
		</attribute>
		<attribute>
			<name>showAvatar</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>affiche ou non l'avatar qui correspond au tiers (défaut=true)</description>
		</attribute>
		<attribute>
			<name>showComplements</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>affiche ou non les informations civiles complémentaires telles que la date de naissance ou les numéros AVS (défaut=false)</description>
		</attribute>
		<attribute>
			<name>urlRetour</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>l'url de retour à l'application appelante (défaut=null)</description>
