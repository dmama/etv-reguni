	<tag>
		<description>Interface sur le service infrastructure</description>
		<name>infra</name>
		<tag-class>ch.vd.uniregctb.taglibs.JspTagInfra</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>le type de l'entité (canton/collectivite/officeImpot/rue)</description>
			<name>entityType</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>l'id de l'entité (numéro Ofs ou numéro postal, selon les cas)</description>
			<name>entityId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>la propriété de l'entité à afficher (nomCourt/dateDebutValidite/...)</description>
			<name>entityPropertyName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>la propriété de l'entité à afficher dans un tooltip</description>
			<name>entityPropertyTitle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Le type d'escaping à appliquer ('none', 'html' ou 'javascript'). Par défaut, c'est 'html'.</description>
			<name>escapeMode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

