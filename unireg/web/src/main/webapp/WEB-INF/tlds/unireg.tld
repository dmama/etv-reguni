			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Le chemin de la propriété à éditer par rapport à la command</description>
			<name>path</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Le type de la propriété à éditer</description>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Le catégorie spécifique de la propriété à éditer (optionel)</description>
			<name>category</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
