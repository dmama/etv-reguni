<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="applicationChecker" class="ch.vd.uniregctb.checker.ApplicationChecker">
		<property name="version" value="${pom.version}" />
		<property name="serviceCivilChecker" ref="serviceCivilChecker" />
		<property name="serviceInfraChecker" ref="serviceInfraChecker" />
		<property name="serviceSecuriteChecker" ref="serviceSecuriteChecker" />
		<property name="serviceBVRChecker" ref="serviceBVRChecker" />
		<property name="serviceEFactureChecker" ref="serviceEFactureChecker" />
		<property name="serviceOrganisationChecker" ref="serviceOrganisationChecker"/>
	</bean>

	<bean id="serviceCivilChecker" class="ch.vd.uniregctb.checker.ServiceCivilChecker">
		<property name="serviceCivilRaw" ref="serviceCivilServiceTracing" />
	</bean>

	<bean id="serviceOrganisationChecker" class="ch.vd.uniregctb.checker.ServiceOrganisationChecker">
		<property name="serviceOrganisationRaw" ref="serviceOrganisationRaw"/>
	</bean>

	<bean id="serviceInfraChecker" class="ch.vd.uniregctb.checker.ServiceInfraChecker">
		<property name="serviceInfraRaw" ref="serviceInfrastructureTracing" />
	</bean>

	<bean id="serviceSecuriteChecker" class="ch.vd.uniregctb.checker.ServiceSecuriteChecker">
		<property name="serviceSecuriteRaw" ref="serviceSecuriteServiceHost" />
	</bean>

	<bean id="serviceBVRChecker" class="ch.vd.uniregctb.checker.ServiceBVRChecker">
		<property name="bvrClient" ref="bvrPlusClient" />
	</bean>

	<bean id="serviceEFactureChecker" class="ch.vd.uniregctb.checker.ServiceEFactureChecker">
		<property name="efactureClient" ref="eFactureClient" />
	</bean>

</beans>
