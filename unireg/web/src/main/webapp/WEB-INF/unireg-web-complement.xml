<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="complementValidator" class="ch.vd.uniregctb.complements.ComplementsValidator" >
		<property name="ibanValidator" ref="ibanValidator" />
		<property name="hibernateTemplate" ref="hibernateTemplate" />
	</bean>

	<bean id="complementController" class="ch.vd.uniregctb.complements.ComplementsController">
		<property name="autorisationManager" ref="autorisationManager"/>
		<property name="controllerUtils" ref="controllerUtils"/>
		<property name="hibernateTemplate" ref="hibernateTemplate"/>
		<property name="validator" ref="complementValidator"/>
	</bean>

</beans>
