<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="efactureController" class="ch.vd.uniregctb.efacture.EFactureController">
	<property name="efactureManager" ref="efactureManager" />
	</bean>

	<bean id="efactureManager" class="ch.vd.uniregctb.efacture.manager.EfactureManagerImpl">
		<property name="tiersService" ref="tiersService" />
		<property name="editiqueCompositionService" ref="editiqueCompositionService" />
	</bean>

</beans>
