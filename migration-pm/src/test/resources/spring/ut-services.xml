<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="rcentEnabled" class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
		<property name="targetClass" value="java.lang.Boolean"/>
		<property name="staticMethod" value="java.lang.Boolean.parseBoolean"/>
		<property name="arguments" value="${rcent.enabled}"/>
	</bean>

	<bean id="serviceOrganisationService" class="ch.vd.uniregctb.interfaces.service.mock.ProxyServiceOrganisation">
		<constructor-arg name="serviceInfra" ref="serviceInfrastructureService"/>
	</bean>

</beans>
