<?xml version="1.0" encoding="UTF-8"?>
<!-- v2.0 - 8 mai 2013 - nouveaux types de débiteurs (PHS, EFF) pour ws-party4 (SIFISC-8625) -->
<!-- v1.5 - 22 août 2012 - ajouté l'element 'status' sur le type 'commonHouseholdType' -->
<!-- v1.2 - 15 décembre 2011 - rendu la documentation compatible avec la javadoc -->
<!-- v1.0beta1 - 1 septembre 2011 - remplacé eCH-0044-2-0f.xsd par eCH-0044-2-0.xsd -->
<!-- v1.0beta0 - 29 juillet 2011 - hand-edited by Manuel Siggen -->
<xs:schema targetNamespace="http://www.vd.ch/fiscalite/unireg/party/person/2" elementFormDefault="qualified"
		   xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eCH-0044="http://www.ech.ch/xmlns/eCH-0044/2"
		   xmlns:common="http://www.vd.ch/fiscalite/unireg/common/1" xmlns:person="http://www.vd.ch/fiscalite/unireg/party/person/2"
		   xmlns:taxpayer="http://www.vd.ch/fiscalite/unireg/party/taxpayer/2" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.0">

	<xs:import namespace="http://www.vd.ch/fiscalite/unireg/common/1" schemaLocation="../unireg-common-1.xsd" />
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044/2" schemaLocation="../eCH-0044-2-0.xsd" />
	<xs:import namespace="http://www.vd.ch/fiscalite/unireg/party/taxpayer/2" schemaLocation="unireg-party-taxpayer-2.xsd" />

	<xs:complexType name="naturalPersonType">
		<xs:annotation>
			<xs:appinfo>
				<jxb:class>
					<jxb:javadoc>Classe concrète qui contient toutes les informations d'un contribuable de type personne physique. Les collections sont renseignées en fonction des parts demandées lors
						de l'appel au service.
					</jxb:javadoc>
				</jxb:class>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="taxpayer:taxpayerType">
				<xs:sequence>
					<xs:element name="identification" type="eCH-0044:personIdentificationType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>Information d'identification de la personne.</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="dateOfBirth" type="common:dateType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>La date de naissance de la personne physique. Cette date peut être partielle, c'est-à-dire que les informations de jour et/ou de mois peuvent valoir 0.
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="dateOfDeath" type="common:dateType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>La date de décès de la personne.</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="category" type="person:naturalPersonCategoryType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>La catégorie de personne physique. Cet attribut ne contient que la valeur courante (= à la date du jour de la requête). Pour l'historique des valeurs,
										voir l'attribut 'categories' (SIFISC-8072).
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="categories" type="person:naturalPersonCategoryPeriodType" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>L'historique complet des catégories de la personne physique. Cet historique est généralement disponible sur les personnes physiques qui habitent dans
										le canton de Vaud. Pour les personnes physiques qui habitent hors-canton ou hors-Suisse, une période unique avec date de début et de fin nulles est retournée.
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="padding" type="xs:int">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>Padding pour supporter l'élément xs:any ci-dessous. A ignorer.</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="naturalPersonCategoryPeriodType">
		<xs:annotation>
			<xs:appinfo>
				<jxb:class>
					<jxb:javadoc>Une catégorie de personne physique valable pendant une période de temps déterminée.</jxb:javadoc>
				</jxb:class>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dateFrom" type="common:dateType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<jxb:property>
							<jxb:javadoc>La date de début de validité de la catégorie. Cette donnée n'est pas renseigné sur les contribuables qui n'habitent pas le canton.</jxb:javadoc>
						</jxb:property>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateTo" type="common:dateType" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<jxb:property>
							<jxb:javadoc>La date de fin de validité de la catégorie. Cette donnée n'est pas renseigné sur les contribuables qui n'habitent pas le canton, ou si la période est toujours
								valable.
							</jxb:javadoc>
						</jxb:property>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="category" type="person:naturalPersonCategoryType">
				<xs:annotation>
					<xs:appinfo>
						<jxb:property>
							<jxb:javadoc>La catégorie de personne physique.</jxb:javadoc>
						</jxb:property>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="commonHouseholdType">
		<xs:annotation>
			<xs:appinfo>
				<jxb:class>
					<jxb:javadoc>Représente un ménage-commun (couple).</jxb:javadoc>
				</jxb:class>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="taxpayer:taxpayerType">
				<xs:sequence>
					<xs:element minOccurs="0" name="mainTaxpayer" type="person:naturalPersonType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>Le composant principal du ménage-commun, ou null si le ménage n'a jamais été rattaché à une personne.
										Attention ! Cette valeur n'est renseignée que si la part HOUSEHOLD_MEMBERS est demandée.
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="secondaryTaxpayer" type="person:naturalPersonType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>Le conjoint du ménage-commun, ou null si le conjoint est inconnu.
										Attention ! Cette valeur n'est renseignée que si la part HOUSEHOLD_MEMBERS est demandée.
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="padding" type="xs:int">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>Padding pour supporter l'élément xs:any ci-dessous. A ignorer.</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="status" type="person:commonHouseholdStatusType">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>L'état du ménage-commun (actif, séparé ou terminé pour cause de décès) ou null si le ménage commun n'a jamais été rattaché à une personne.
										Attention ! Ce status n'est renseigné que si la part HOUSEHOLD_MEMBERS est demandée.
									</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element name="padding2" type="xs:int">
						<xs:annotation>
							<xs:appinfo>
								<jxb:property>
									<jxb:javadoc>Padding pour supporter l'élément xs:any ci-dessous. A ignorer.</jxb:javadoc>
								</jxb:property>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="commonHouseholdStatusType">
		<xs:annotation>
			<xs:appinfo>
				<jxb:typesafeEnumClass>
					<jxb:javadoc>L'état d'un ménage-commun.</jxb:javadoc>
				</jxb:typesafeEnumClass>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ACTIVE">
				<xs:annotation>
					<xs:documentation>Le ménage est toujours actif (marié, pacsé, ...).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SEPARATED_DIVORCED">
				<xs:annotation>
					<xs:documentation>Le ménage est terminé suite à une séparation ou un divorce (ou à une interruption de pacs).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ENDED_BY_DEATH">
				<xs:annotation>
					<xs:documentation>Le ménage est terminé suite au décès d'un (ou des deux) des conjoints.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="sexType">
		<xs:annotation>
			<xs:appinfo>
				<jxb:typesafeEnumClass>
					<jxb:javadoc>Sexe d'une personne physique.</jxb:javadoc>
				</jxb:typesafeEnumClass>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FEMALE">
				<xs:annotation>
					<xs:documentation>Féminin.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MALE">
				<xs:annotation>
					<xs:documentation>Masculin.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="naturalPersonCategoryType">
		<xs:annotation>
			<xs:appinfo>
				<jxb:typesafeEnumClass>
					<jxb:javadoc>Les différentes catégories d'une personne physique.</jxb:javadoc>
				</jxb:typesafeEnumClass>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SWISS">
				<xs:annotation>
					<xs:documentation>Suisse.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_02_B_PERMIT">
				<xs:annotation>
					<xs:documentation>Permis de séjour (B).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_03_C_PERMIT">
				<xs:annotation>
					<xs:documentation>Permis d'établissement (C).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_04_CI_PERMIT">
				<xs:annotation>
					<xs:documentation>Permis de conjoint de diplomate (CI).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_05_F_PERMIT">
				<xs:annotation>
					<xs:documentation>Etranger admis provisoirement (F).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_06_G_PERMIT">
				<xs:annotation>
					<xs:documentation>Frontalier (G).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_07_L_PERMIT">
				<xs:annotation>
					<xs:documentation>Permis de séjour de courte durée (L).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_08_N_PERMIT">
				<xs:annotation>
					<xs:documentation>Requérant d'asile (N).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_09_S_PERMIT">
				<xs:annotation>
					<xs:documentation>Personne à protéger (S).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_10_OBLIGED_TO_ANNOUNCE">
				<xs:annotation>
					<xs:documentation>Personne tenue de s'annoncer.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_11_DIPLOMAT">
				<xs:annotation>
					<xs:documentation>Diplomate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_12_INTERNATIONAL_CIVIL_SERVANT">
				<xs:annotation>
					<xs:documentation>Fonctionnaire international.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_13_NOT_ASSIGNED">
				<xs:annotation>
					<xs:documentation>Non attribuée.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
