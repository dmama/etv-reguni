<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean name="editiqueListener" class="ch.vd.watchdog.editique.ListenerImpl" parent="esbMessageListener">
	</bean>

	<bean id="evenementEditiqueEndpointManager" class="ch.vd.technical.esb.jms.EsbMessageEndpointManager">
		<property name="resourceAdapter" ref="jmsResourceAdapter"/>
		<property name="activationSpec">
			<bean class="org.apache.activemq.ra.ActiveMQActivationSpec">
				<property name="destination" value="unireg.retourImpression-WATCHDOG"/>
				<property name="destinationType" value="javax.jms.Queue"/>
				<property name="maxSessions" value="1"/>
				<property name="maxMessagesPerSessions" value="1"/>
			</bean>
		</property>
		<property name="messageListener" ref="editiqueListener"/>
	</bean>

</beans>
