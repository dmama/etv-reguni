	<xs:complexType name="typAdresse">
		<xs:annotation>
			<xs:documentation>version 1.1 au 19.05.04. corrige le noeud all en sequence car problème d'extension d'un noeud "all" pour certains parsers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="adresse">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="adresseCourrierLigne1" type="typCdata">
							<xs:annotation>
								<xs:documentation>e.g. Monsieur, Madame</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="adresseCourrierLigne2" type="typCdata">
							<xs:annotation>
								<xs:documentation>Ligne adresse 2</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="adresseCourrierLigne3" type="typCdata">
							<xs:annotation>
								<xs:documentation>Ligne adresse 3</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="adresseCourrierLigne4" type="typCdata">
							<xs:annotation>
								<xs:documentation>Ligne adresse 4</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="adresseCourrierLigne5" type="typCdata">
							<xs:annotation>
								<xs:documentation>Ligne adresse 5</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="adresseCourrierLigne6" type="typCdata">
							<xs:annotation>
								<xs:documentation>Ligne adresse 6</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typPeriode">
		<xs:annotation>
			<xs:documentation>Entete de document</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="prefixe">
				<xs:annotation>
					<xs:appinfo/>
					<xs:documentation>RGPX0701PERIO</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="13"/>
						<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OrigDuplicat">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:pattern value="ORG|DPL|SIM|PDS"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HorsSuisse" type="typOui">
				<xs:annotation>
					<xs:documentation>O ou blanc</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HorsCanton" type="typOui">
				<xs:annotation>
					<xs:documentation>O ou blanc</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AnneeFiscale" type="typAnnee_Vide"/>
			<xs:element name="DateDecompte" type="typDatAAAAMMJJ_Vide">
				<xs:annotation>
					<xs:documentation>AAAAMMJJ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Entete">
				<xs:complexType>
					<xs:sequence maxOccurs="unbounded">
						<xs:element name="Tit" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="prefixe">
										<xs:annotation>
											<xs:appinfo/>
											<xs:documentation>RGPX0701TITIM</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="13"/>
												<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="LibTit" type="xs:string">
										<xs:annotation>
											<xs:documentation>Ex: IMPOT SUR LE REVENU ET LA FORTUNE 2006</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ImpCcn" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="prefixe">
										<xs:annotation>
											<xs:appinfo/>
											<xs:documentation>RGPX0701IMPCC</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="13"/>
												<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="LibImpCcn" type="xs:string">
										<xs:annotation>
											<xs:documentation>Ex: Décompte final 2006</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="DatDerCalculAc" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SIPF : Annule et remplace le calcul des acomptes 20.02.2008</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typEntete">
		<xs:sequence>
			<xs:element ref="prefixe"/>
		</xs:sequence>
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>Eléments complexes streamserve communs</xs:documentation>
	</xs:annotation>
	<xs:element name="infoTechnique">
		<xs:annotation>
			<xs:documentation>TECHN~Entête technique du fichier TECHN. version 1.1 du 19.04.04, suppression du XML dans le nom de fichier</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="prefixe">
					<xs:annotation>
						<xs:documentation>RGPX0701TECHN</xs:documentation>
						<xs:appinfo/>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="13"/>
							<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nomFichier" type="xs:string">
					<xs:annotation>
						<xs:documentation>Nom du fichier au format XXPxBaammjj_hhmmss000</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="sigleApplication" minOccurs="0"/>
				<xs:element name="idUtilisateur" type="xs:token" minOccurs="0">
					<xs:annotation>
						<xs:documentation>ID2005 ~ nom ou code de l'utilisateur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="codTraitement"/>
				<xs:element name="codSpecimen" type="xs:token"/>
				<xs:element name="codRecommande" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:enumeration value="O"/>
							<xs:enumeration value="N"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="premNumRecommande" default="0" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Obligatoire si recommandés, sinon = 0 </xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:minInclusive value="0"/>
							<xs:totalDigits value="8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="derNumRecommande" default="0" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Obligatoire si recommandés, sinon = 0 </xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:int">
							<xs:minInclusive value="0"/>
							<xs:totalDigits value="8"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="idXmlAggrege" type="xs:string" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="infoRoutage">
		<xs:annotation>
			<xs:documentation>Pied de fichier pour routage batch ROUTE</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="prefixe">
					<xs:annotation>
						<xs:documentation>RGPX0701ROUTE</xs:documentation>
						<xs:appinfo/>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="13"/>
							<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nbDocuments" type="xs:int"/>
				<xs:element ref="codTraitement"/>
				<xs:element ref="sigleApplication"/>
				<xs:element name="titre" type="xs:string">
					<xs:annotation>
						<xs:documentation>"Application émettrice"</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="datExpedition">
					<xs:annotation>
						<xs:documentation>Date d'expédition</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:pattern value="[0-9]{1,2}\.[0-9]{1,2}\.[0-9]{4}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="typPapier" type="xs:int" default="703">
					<xs:annotation>
						<xs:documentation>No du papier</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="typEnveloppe" type="xs:int" default="022">
					<xs:annotation>
						<xs:documentation>No type enveloppe</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="infoAnnexe" minOccurs="7" maxOccurs="7"/>
				<xs:element name="destination" type="xs:string"/>
				<xs:element ref="remarque" minOccurs="2" maxOccurs="2"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="infoDocument">
		<xs:annotation>
			<xs:documentation>Informations d'identification du document DOCUM.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="prefixe">
					<xs:annotation>
						<xs:documentation>RGPX0701DOCUM</xs:documentation>
						<xs:appinfo/>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="13"/>
							<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="codPorteAdresse" type="xs:string" nillable="true" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Code porte-adresse {'M'}=avec page de garde sinon {' '}</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="txtCopieMandataire" type="xs:string" minOccurs="0" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>Texte copie</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="idEnvoi" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Code ou identifiant de l'envoi, permet de déterminer quand on commence un nouvel envoi (cas spéciaux)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="typDoc" type="xs:string">
					<xs:annotation>
						<xs:documentation>Exemple : DT / FA / DF</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="codDoc" type="xs:string">
					<xs:annotation>
						<xs:documentation>ICC_IFD_ALL</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="cleRgp"/>
				<xs:element name="version" type="xs:string">
					<xs:annotation>
						<xs:documentation>V 1.0</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="logo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>CONF/CANT</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="CONF|CANT"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="populations" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Indication si PP / IS / PM ou non comuniqué</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="PP|IS|PM|.{0}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="infoArchivage">
		<xs:annotation>
			<xs:documentation>Informations pour l'archivage dans FOLDERS</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="prefixe">
					<xs:annotation>
						<xs:documentation>RGPX0701FOLDE</xs:documentation>
						<xs:appinfo/>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="13"/>
							<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nomApplication" type="xs:string" fixed="FOLDERS"/>
				<xs:element name="typDossier" type="xs:string" fixed="003">
					<xs:annotation>
						<xs:documentation>Attribué par l'Editique</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="nomDossier" type="xs:string">
					<xs:annotation>
						<xs:documentation>numéro du contribuable avec un point comme séparateur</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="typDocument" type="xs:string">
					<xs:annotation>
						<xs:documentation>Code du type de document attribué par l'Editique</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="idDocument">
					<xs:annotation>
						<xs:documentation>Nom FOLDERS du document</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="40"/>
							<xs:pattern value="[0-9]{4}( |[1-9]){1}[0-9]{1} (\w|\W){19} [0-9]{13}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="datTravail" type="typDatAAAAMMJJ">
					<xs:annotation>
						<xs:documentation>Date de travail, au format aaaammjj : date d'expédition d'une correspondance</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="infoEnteteDocument">
		<xs:annotation>
			<xs:documentation>Informations de l'entête de chaque document HAUT1</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="prefixe">
					<xs:annotation>
						<xs:documentation>RGPX0701HAUT1</xs:documentation>
						<xs:appinfo/>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:length value="13"/>
							<xs:pattern value="RG[A-Z][A-Z][0-9]{2}[0-9]{2}\w{5}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="expediteur">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="typAdresse">
								<xs:sequence>
									<xs:element name="adrMes" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>adresse messagerie</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="numTelephone" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>no Tél.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="numFax" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>no Fax</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="numCCP" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>no CCP</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="traitePar" type="xs:string">
										<xs:annotation>
											<xs:documentation>Titre, initiales et nom de l'utilisateur</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="srvExp" type="xs:string" minOccurs="0"/>
									<xs:element name="ideUti" type="xs:string">
										<xs:annotation>
											<xs:documentation>Visa utilisateur </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="localiteExpedition" type="typCdata" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Localité </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="dateExpedition" type="typDatAAAAMMJJ" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Date d'expédition au format jj MMMM aaaa (13 janvier 2005)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="notreReference" type="xs:token" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Texte notre référence</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="votreReference" type="typCdata" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Texte votre référence</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="numIBAN" type="typCdata" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Texte votre référence</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="destinataire">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="typAdresse">
								<xs:sequence>
									<xs:element name="numRecommande" type="xs:string"/>
									<xs:element name="numContribuable" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Identifiant, e.g. no de la personne</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="porteAdresse" type="typAdresse"/>
				<xs:element name="ligReference" type="typCdata" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Ligne de référence (e.g. concerne)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:all>
