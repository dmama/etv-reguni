<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="DonneesTechniques_0.0.2.xsd"/>
	<xs:element name="DIVDTAX">
		<!-- Description du schema pour l'impression du document BVR-->
		<xs:complexType>
			<xs:sequence>
				<xs:element name="InfoDI">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ANNEEFISCALE" type="xs:string">
								<xs:annotation>
									<xs:documentation>Année fiscal du traitement</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DESCOM" type="xs:string">
								<xs:annotation>
									<xs:documentation>Désignation commune (nom OFS majuscules)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="DELAIRETOUR" type="typDatAAAAMMJJ" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Délai de retour de la DI (JJ.MM.AAAA)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="NOCANT" type="xs:string">
								<xs:annotation>
									<xs:documentation>Numéro contribuable édité</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="NOOID" type="typEntier">
								<xs:annotation>
									<xs:documentation>no office  (prendre les 2 dernières positions pour l'impression)  </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CODBARR" type="typEntier">
								<xs:annotation>
									<xs:documentation>Code à barres (128) :  no ctb(9), AAAA, no ordinal(2), No OID(2)</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="AdresseRetour">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ADRES1RETOUR" type="xs:string">
								<xs:annotation>
									<xs:documentation>Adresse 1 CEDI </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ADRES2RETOUR" type="xs:string">
								<xs:annotation>
									<xs:documentation>Adresse 2 CEDI </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ADRES3RETOUR" type="xs:string">
								<xs:annotation>
									<xs:documentation>Adresse 3 CEDI </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ADRES4RETOUR" type="xs:string">
								<xs:annotation>
									<xs:documentation>Adresse 4 CEDI </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ADRES5RETOUR" type="typCdata">
								<xs:annotation>
									<xs:documentation>Adresse 5 CEDI </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ADRES6RETOUR" type="typCdata">
								<xs:annotation>
									<xs:documentation>Adresse 6 CEDI </xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Contrib1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="INDETATCIVIL1" type="typCdata">
								<xs:annotation>
									<xs:documentation>Etat civil de l'individu1</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="INDNOMPRENOM1" type="typCdata">
								<xs:annotation>
									<xs:documentation>Prénom et Nom de l'individu1</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="INDDATENAISS1" type="typCdata">
								<xs:annotation>
									<xs:documentation>Date de naissance de l'individu1</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="NAVS13" type="xs:string">
								<xs:annotation>
									<xs:documentation>Num AVS</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Contrib2">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="INDETATCIVIL2" type="typCdata">
								<xs:annotation>
									<xs:documentation>Etat civil de l'individu2</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="INDNOMPRENOM2" type="typCdata">
								<xs:annotation>
									<xs:documentation>Prénom et Nom de l'individu2</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="INDDATENAISS2" type="typCdata">
								<xs:annotation>
									<xs:documentation>Date de naissance de l'individu2 </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="NAVS13" type="xs:string">
								<xs:annotation>
									<xs:documentation>Num AVS</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Annexes">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Annexe_200" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>200 = H.C avec le nombre de doc 1àN</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_210" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>210 = DI Standard avec le nombre de doc 1àN </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_220" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>220 = Annexe 01 avec le nombre de doc 1àN </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_230" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>230 = Annexe 02 – 03 avec le nombre de doc 1àN </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_240" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>240 = Annexe 04–05 avec le nombre de doc 1àN  </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_250" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>250 = VaudTax avec le nombre de doc 1àN   </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_270" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>270 = DI – Dépence avec le nombre de doc 1àN   </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Annexe_310" type="typEntier" minOccurs="0">
								<xs:annotation>
									<xs:documentation>310 = Annexe 01–1(2009) avec le nombre de doc 1àN   </xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
