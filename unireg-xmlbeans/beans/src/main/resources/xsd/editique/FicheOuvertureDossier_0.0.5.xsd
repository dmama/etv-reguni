								</xs:annotation>
							</xs:element>
							<xs:element name="NumAVS132" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Num AVS 13</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DateEdition" type="typDatAAAAMMJJ">
					<xs:annotation>
						<xs:documentation>Date d' édition</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
