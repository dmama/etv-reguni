	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:element name="AbstractGebaeude" type="grundstueck:AbstractGebaeudeType"/>
	<xs:complexType name="AbstractGebaeudeType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Attributs d'un bâtiment.</xs:documentation>
			<xs:documentation xml:lang="de">Attribute von Gebaeude.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GrundstueckZuGebaeude" type="grundstueck:GrundstueckZuGebaeudeType" maxOccurs="unbounded"/>
			<xs:element name="GwrEgid" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="KantGid" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="GemeindeGid" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Einzelobjekt" type="xs:boolean"/>
			<xs:element name="EinzelobjektArt" type="grundstueck:EinzelobjektArtType" minOccurs="0"/>
			<xs:element name="FlaecheEinzelobjekt" type="xs:decimal" minOccurs="0"/>
			<xs:element name="Unterirdisch" type="xs:boolean"/>
			<xs:element name="MehrereGrundstuecke" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Flaeche" type="xs:decimal" minOccurs="0"/>
			<xs:element name="Koordinaten" type="grundstueck:MinMaxKoordiatenType" minOccurs="0"/>
			<!-- Nur 8 Gebaeude haben mehr als 15 Adressen, ~90% der Gebaeude haben nur 1 Adresse -->
			<xs:element name="GebaeudeAdressen" type="grundstueck:GebaeudeAdresseType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
