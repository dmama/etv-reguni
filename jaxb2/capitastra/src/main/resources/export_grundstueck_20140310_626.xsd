	<xs:complexType name="ENKoordiatenType">
		<xs:sequence>
			<xs:element name="N" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="E" type="xs:unsignedInt" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:complexType name="GebaeudeAdresseType">
		<xs:annotation>
			<xs:documentation>eine Gebäudeadresse</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GwrEdid" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Strasse" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ein Strassenname</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Hausnummer" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>eine Hausnummer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Plz" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>eine Postleitzahl</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlzZusatz" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Zusatz zu einer Postleitzahl</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Ort" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Un nom de lieu.</xs:documentation>
					<xs:documentation xml:lang="de">ein Ortsname.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EingangBezeichung" type="xs:string" minOccurs="0"/>
			<xs:element name="LaufNr" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Koordinaten" type="grundstueck:ENKoordiatenType" minOccurs="0"/>
			<xs:element name="Lokalisation" type="grundstueck:LokalisationType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:complexType name="GrundstueckZuGebaeudeType">
		<xs:sequence>
			<!-- 58% der Grundstücke haben nur ein Gebaeude, 0.4% der Grundstücke haben mehr als 10 Gebaeude. -->
			<xs:element name="GrundstueckIDREF" type="xs:IDREF">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique sur un immeuble. Lie un bâtiment à l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Referenze auf eines Grundstück. Verbindet Gebaeude mit Grundstueck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AbschnittFlaeche" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Dans le cas où le bâtiment se situe sur plusieurs immeubles, cet attribut contient la surface occupée sur le présent immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Falls das Gebäude auf mehreren Grundstücken liegt, enthält dieses Attribut die Fläche, welche auf dem aktuellen Grundstück liegt.</xs:documentation>
				</xs:annotation>
				<!-- Kommt aus der Tabelle GebaeudeAufGrundstueck -->
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- Meinstens Gibt es nur 1 Gebaeude pro Grundstück, so mit GrundstueckIDREF werden die Gebauede Daten fast nicht dupliziert  -->
	<!-- 97% der Gebaeude liegen nur auf 1 Grundstück. Nur 144 Gebäude liegen auf mehr als 10 Grundstücke. -->
	<xs:element name="Gebaeude" type="grundstueck:GebaeudeType" substitutionGroup="grundstueck:AbstractGebaeude"/>
	<xs:complexType name="GebaeudeType">
		<xs:complexContent>
			<xs:extension base="grundstueck:AbstractGebaeudeType">
				<xs:sequence>
					<!-- 99% der Gebaeude haben nur 1 GebaeudeArt, maximum ist 4 GebaeudeArt bei 5 Gebaeude. -->
					<xs:element name="GebaeudeArten" type="grundstueck:GebaeudeArtType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Versicherungsnummer" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Le numéro d'assurance du bâtiment.</xs:documentation>
							<xs:documentation xml:lang="de">Die Versicherungsnummer des Gebäudes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="VersionID" type="xs:string" use="optional"/>
				<xs:attribute name="MasterID" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- Meinstens Gibt es nur 1 Gebaeude pro Grundstück, so mit GrundstueckIDREF werden die Gebauede Daten fast nicht dupliziert  -->
	<xs:element name="ProjektiertGebaeude" type="grundstueck:ProjektiertGebaeudeType" substitutionGroup="grundstueck:AbstractGebaeude"/>
	<xs:complexType name="ProjektiertGebaeudeType">
