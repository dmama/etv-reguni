			<xs:element name="UeberlagerndeRechte" type="xs:boolean" minOccurs="0"/>
			<!-- AV -->
			<xs:element name="IndexBS" type="xs:unsignedInt" minOccurs="0"/>
			<!-- Basel -->
			<xs:element name="StatusAV" type="grundstueck:StatusAVType" minOccurs="0"/>
			<xs:element name="QualitaetAV" type="grundstueck:QualitaetAVType" minOccurs="0"/>
			<xs:element name="FlaechenQualitaet" type="grundstueck:FlaechenQualitaetType" minOccurs="0"/>
			<!-- 90% der Grundstücksflächen haben nur 1 Rechtsgrund -->
			<xs:element name="Rechtsgruende" type="grundstueck:RechtsgrundType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
		<xs:attribute name="MasterID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- MinMaxKoordinatenType                                             -->
	<!-- ********************************************************** -->
	<xs:complexType name="MinMaxKoordiatenType">
		<xs:sequence>
			<xs:element name="MinN" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="MinE" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="MaxN" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="MaxE" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Z" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="QualitaetKoordinaten" type="grundstueck:QualitaetKoordinatenType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- GrundstueckBemerkungType                                      -->
	<!-- ********************************************************** -->
	<!-- Dieser Typ wurde erstellt, um GrundstueckBemerkung liefern zu können oder nicht. -->
	<xs:complexType name="GrundstueckBemerkungType">
		<xs:sequence>
			<xs:element name="Text" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- GRUNDSTUECK                                                         -->
	<!-- ********************************************************** -->
	<xs:simpleType name="GrundstueckArtType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Liegenschaft"/>
			<xs:enumeration value="StockwerksEinheit"/>
			<xs:enumeration value="GewoehnlichesMiteigentum"/>
			<xs:enumeration value="Bergwerk"/>
			<xs:enumeration value="Folio">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Immeuble de type Folio, voire Seybuch, uniquement pour Berne.</xs:documentation>
					<xs:documentation xml:lang="de">Folio Grundstück bzw. Seybuch, nur bei Bern.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SelbstaendigesDauerndesRecht">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Droit distinct et permanent DDP.</xs:documentation>
					<xs:documentation xml:lang="de">Selbständiges und dauerndes Recht SdR.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UnbekanntesGrundstueck"/>
		</xs:restriction>
		<!-- Entspricht DB -->
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:complexType name="GrundstueckKopfType">
		<xs:sequence>
			<xs:element name="EGrid" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro fédéral d'identification de l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Eidgenössiche Grundstücksidentifikationsnummer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrundstueckNummer" type="grundstueck:GrundstueckNummerType"/>
			<xs:element name="GrundstueckArt" type="grundstueck:GrundstueckArtType"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:complexType name="GrundstueckNummerType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">l'identification d'un immeuble.</xs:documentation>
			<xs:documentation xml:lang="de">Die Identifikationsnummer eines Grundstücks.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BfsNr" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro OFS de la commune.</xs:documentation>
					<xs:documentation xml:lang="de">BFS-Nummer der Gemeinde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Gemeindenamen" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Nom de la commune.</xs:documentation>
					<xs:documentation xml:lang="de">Namen der Gemeinde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KreisNr" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro d'arrondissement de la commune.</xs:documentation>
					<xs:documentation xml:lang="de">Kreisnummer der Gemeinde.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StammNr" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro de base de l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Stammnummer des Grundstücks.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndexNr1" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Premier index d'un immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Erster Index des Grundstücks.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndexNr2" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Deuxième index d'un immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Zweiter Indexdes Grundstücks.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndexNr3" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Troisième index d'un immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Dritter Indexdes Grundstücks.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="Grundstueck" type="grundstueck:GrundstueckType"/>
	<xs:complexType name="GrundstueckType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Toutes les données qui décrivent un immeuble.</xs:documentation>
			<xs:documentation xml:lang="de">Alle Daten, die ein Grundstueck beschrieben.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GrundstueckID" type="xs:ID">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique d'un immeuble. Par ce biais des relations peuvent être établies entre par exemple l'immeuble et son adresse.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Identifikation eines Grundstück. Damit können die Beziehungen berechnet werde, zB. zwischen Grundstück und seine Adresse.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EGrid" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro fédéral de l'identification de l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Eidgenössiche Grundstücksidentifikationsnummer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrundstueckNummer" type="grundstueck:GrundstueckNummerType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Le numéro d'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Die Grundstücksnummer zu diesem Grundstück.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnterArt" type="grundstueck:CapiCodeType" minOccurs="0"/>
			<xs:element name="IstKopie" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Ce fanion indique si l'immeuble n'existe qu'à l'état de copie, dans ce cas, l'original se trouve en dehors des données actuelles.</xs:documentation>
					<xs:documentation xml:lang="de">istKopie bezeichnet Grundstücke, welche nur als Kopie vorhanden sind. Das Original befindet sich ausserhalb des Datenbestandes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AmtlicheBewertung" type="grundstueck:AmtlicheBewertungType" minOccurs="0"/>
			<xs:element name="Bemerkung" type="grundstueck:GrundstueckBemerkungType" minOccurs="0"/>
			<!-- -->
			<xs:element name="GrundbuchFuehrung" type="xs:string" minOccurs="0"/>
			<xs:element name="BeschreibungUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="EigentumUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="PfandrechtUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="DienstbarkeitUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="GrundlastUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="AnmerkungUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="VormerkungUebergeben" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Bereinigungsmarkierung" type="xs:boolean" minOccurs="0"/>
			<xs:element name="BereitZurVerifikation" type="xs:boolean" minOccurs="0"/>
			<xs:element name="NutzungLandwirtschaft" type="grundstueck:JaNeinUnbekanntType" minOccurs="0"/>
			<xs:element name="NutzungWald" type="grundstueck:JaNeinUnbekanntType" minOccurs="0"/>
			<xs:element name="NutzungEisenbahn" type="grundstueck:JaNeinUnbekanntType" minOccurs="0"/>
			<xs:element name="NutzungVerwaltungsvermoegen" type="grundstueck:JaNeinUnbekanntType" minOccurs="0"/>
			<xs:element name="Bereinigungsgrund" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="Liegenschaft" type="grundstueck:LiegenschaftType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="LiegenschaftType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un immeuble de type bien-fond.</xs:documentation>
			<xs:documentation xml:lang="de">Ein Liegenschaft Grundstück.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType">
				<xs:sequence>
					<xs:element name="Koordinaten" type="grundstueck:MinMaxKoordiatenType" minOccurs="0"/>
					<!-- Da diese Daten mit einem Subselect geladen werden, macht es  keinen Unterschied mit einem separatem Laden. -->
					<xs:element name="GrundstueckFlaeche" type="grundstueck:GrundstueckFlaecheType" minOccurs="0"/>
					<xs:element name="QualitaetBodenbedeckung" type="grundstueck:QualitaetBodenbedeckungType" minOccurs="0"/>
					<xs:element name="AltePlanAenderungsnummer" type="xs:string" minOccurs="0"/>
					<xs:element name="LigUnterartEnum" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>	
	<!-- ********************************************************** -->
	<xs:complexType name="KonzessionsDatumType">
		<xs:sequence>
			<xs:element name="Tag" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Monat" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Jahr" type="xs:unsignedInt" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:simpleType name="RechtArtType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="bauRecht"/>
			<xs:enumeration value="quellenRecht"/>
			<xs:enumeration value="konzessionRecht"/>
			<xs:enumeration value="uebrigesRecht"/>
		</xs:restriction>
		<!-- Entspricht DB -->
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:element name="SDR" type="grundstueck:SDRType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="SDRType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un immeuble de type de droit distinct et permanent.</xs:documentation>
			<xs:documentation xml:lang="de">Ein SelbstaendigesDauerndesRecht Grundstück.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType">
				<xs:sequence>
					<xs:element name="Koordinaten" type="grundstueck:MinMaxKoordiatenType" minOccurs="0"/>
					<xs:element name="GrundstueckFlaeche" type="grundstueck:GrundstueckFlaecheType" minOccurs="0"/>
					<xs:element name="AltePlanAenderungsnummer" type="xs:string" minOccurs="0"/>
					<xs:element name="BeschreibungKonzession" type="xs:string" minOccurs="0"/>
					<xs:element name="KonzessionAblaufdatum" type="grundstueck:KonzessionsDatumType" minOccurs="0"/>
					<xs:choice minOccurs="0">
						<xs:element name="RechtArt" type="grundstueck:RechtArtType">
							<xs:annotation>
								<xs:documentation xml:lang="fr">Seulement pour le canton de Berne.</xs:documentation>
								<xs:documentation xml:lang="de">Nur für Kanton Bern.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CodeSdR" type="grundstueck:CapiCodeType">
							<xs:annotation>
								<xs:documentation xml:lang="fr">Pour tous les cantons à l'exception de celui de Berne.</xs:documentation>
								<xs:documentation xml:lang="de">Für alle Kantonen ausser Bern.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="StockwerksEinheit" type="grundstueck:StockwerksEinheitType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="StockwerksEinheitType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un immeuble de type propriété par étage PPE.</xs:documentation>
			<xs:documentation xml:lang="de">Ein StockwerksEinheit Grundstück.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType">
				<xs:sequence>
					<xs:element name="Sonderrecht" type="grundstueck:SonderrechtType" minOccurs="0"/>
					<xs:element name="StammGrundstueck" type="grundstueck:StammGrundstueckType"/>
					<xs:element name="BeschreibungMiteigentum" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="GewoehnlichesMiteigentum" type="grundstueck:GewoehnlichesMiteigentumType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="GewoehnlichesMiteigentumType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un immeuble de type copropriété.</xs:documentation>
			<xs:documentation xml:lang="de">Ein GewoehnlichesMiteigentum Grundstück.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType">
				<xs:sequence>
					<xs:element name="StammGrundstueck" type="grundstueck:StammGrundstueckType"/>
					<xs:element name="BeschreibungMiteigentum" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="Folio" type="grundstueck:FolioType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="FolioType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">un immeuble de type folio, seulement pour Berne.</xs:documentation>
			<xs:documentation xml:lang="de">Ein Folio Grundstück, nur bei Bern.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType">
				<xs:sequence>
					<xs:element name="QuoteKuhRecht" type="grundstueck:QuoteKuhRechtType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="Bergwerk" type="grundstueck:BergwerkType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="BergwerkType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un immeuble de type mine.</xs:documentation>
			<xs:documentation xml:lang="de">Ein Bergwerk Grundstück.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType">
				<xs:sequence>
					<xs:element name="Koordinaten" type="grundstueck:MinMaxKoordiatenType" minOccurs="0"/>
					<xs:element name="GrundstueckFlaeche" type="grundstueck:GrundstueckFlaecheType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="UnbekanntesGrundstueck" type="grundstueck:UnbekanntesGrundstueckType" substitutionGroup="grundstueck:Grundstueck"/>
	<xs:complexType name="UnbekanntesGrundstueckType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un immeuble de type inconnu.</xs:documentation>
			<xs:documentation xml:lang="de">Ein unbekanntes Grundstück.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckType"/>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
