					<xs:element name="BaubewilligungsID" type="xs:string" minOccurs="0"/>
					<xs:element name="DatumErteilung" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Date de remise de l'autorisation de bâtir.</xs:documentation>
							<xs:documentation xml:lang="de">Datum der Erteilung der Baubewilligung.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Bemerkung" type="xs:string" minOccurs="0"/>
					<xs:element name="projektiertStatus" type="grundstueck:ProjektiertStatusType"/>
				</xs:sequence>
				<xs:attribute name="VersionID" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
