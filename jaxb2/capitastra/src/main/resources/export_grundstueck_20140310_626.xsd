				<xs:sequence>
					<xs:element name="Sitz" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Siège de la société.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Gesellschaftsform" type="grundstueck:CapiCodeType" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Forme de la société.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Rechtsform" type="grundstueck:CapiCodeType" minOccurs="0"/>
					<xs:element name="RechtsformZusatz" type="xs:string" minOccurs="0"/>
					<xs:element name="FirmenNr" type="xs:string" minOccurs="0"/>
					<xs:element name="Departement" type="xs:string" minOccurs="0"/>
					<xs:element name="NoCollectivitePublic" type="xs:decimal" minOccurs="0"/>
					<!-- Genf -->
					<xs:element name="Unterart" type="grundstueck:JuristischePersonUnterartType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="PersonstammKantonaleIdentifikation" type="grundstueck:PersonstammKantonaleIdentifikationType"/>
	<xs:complexType name="PersonstammKantonaleIdentifikationType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Définit l'identification d'une personne de base, varie selon le canton.</xs:documentation>
			<xs:documentation xml:lang="de">Definiert die Identifikation einer Stammperson, abhängig vom Kanton.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PersonstammID" type="xs:ID">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Identification technique d'une personne.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Identifikation einer Stammperson</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="KantonalPersonNr" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro cantonal unique d'une personne. Par exemple, le numéro GCP dans le canton de Berne.</xs:documentation>
					<xs:documentation xml:lang="de">Kantonsweit eindeutige Personennummer. Z.B. ZPV Nummer im Kanton BE.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:sequence minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Seulement pour le canton de Vaud.</xs:documentation>
					<xs:documentation xml:lang="de">Nur Bei Kanton Waadt.</xs:documentation>
				</xs:annotation>
				<xs:element name="NrACI" type="xs:decimal" minOccurs="0">
					<xs:annotation>
							<xs:documentation xml:lang="fr">Seulement pour le canton de Vaud, numéro ACI</xs:documentation>
							<xs:documentation xml:lang="de">Nur fuer Kanton VD.</xs:documentation>
						</xs:annotation>
				</xs:element>
				<xs:element name="NrIROLE" type="xs:decimal" minOccurs="0">
					<xs:annotation>
							<xs:documentation xml:lang="fr">Seulement pour le canton de Vaud, numéro IROLE.</xs:documentation>
							<xs:documentation xml:lang="de">Nur fuer Kanton VD.</xs:documentation>
						</xs:annotation>
				</xs:element>
				<xs:element name="NoRF" type="xs:decimal" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Seulement pour le canton de Vaud. Numéro qui correspond au capiPersonenID.</xs:documentation>
						<xs:documentation xml:lang="de">Nur fuer Kanton VD. Nummer, welcher der capiPersonenID entspricht.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- GRUNDSTUECK                                                                                                                        -->
	<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ********************************************************** -->
	<!-- GRUNDSTUECKEIGENTUMANTEIL                        -->
	<!-- ********************************************************** -->
	<xs:simpleType name="GrundstueckEigentumsformType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Stockwerk"/>
			<xs:enumeration value="Miteigentum"/>
			<xs:enumeration value="DominierendesGrundstueck"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:element name="GrundstueckEigentumAnteil" type="grundstueck:GrundstueckEigentumAnteilType" substitutionGroup="grundstueck:EigentumAnteil"/>
	<xs:complexType name="GrundstueckEigentumAnteilType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Part de propriété entre l'immeuble grevé et l'immeuble bénéficiaire.</xs:documentation>
			<xs:documentation xml:lang="de">Eigentumsanteil mit einem beteiligten Grundstück und Eigentümer (berechtigtem Grundstück).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:EigentumAnteilType">
				<xs:sequence>
					<xs:element name="BerechtigtesGrundstueckIDREF" type="xs:IDREF">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Référence technique sur un immeuble bénéficiaire.</xs:documentation>
							<xs:documentation xml:lang="de">Technische Referenze auf ein berechtigtes Grundstück (Eigentümer).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GrundstueckEigentumsForm" type="grundstueck:GrundstueckEigentumsformType">
						<xs:annotation>
							<xs:documentation xml:lang="fr">Information d'aide pour simplifier le traitement de la propriété. Les valuers suivantes sont possibles:
							STW: Propriété par étage.
							MIT: Copropriété.
							DominierendesGS: Immeuble bénéficiaire.
							</xs:documentation>
							<xs:documentation xml:lang="de">Hilfsinformation zur einfacheren Verarbeitung des Eigentums. Folgende Werte sind möglich:
							STW: Beim Grundstück handelt es sich um ein Stockwerk.
							MIT: Beim Grundstück handelt es sich um ein (gewöhnliches) Miteigentum.
							DominierendesGS: Beim Grundstück handelt es sich um ein dominiereneds Grundstück.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- 80% der Eigentümer haben nur 1 Rechtsgrund, 0.03% haben mehrs als 10 (max. 22). -->
					<xs:element name="Rechtsgruende" type="grundstueck:RechtsgrundType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="IndexBS" type="xs:unsignedInt" minOccurs="0"/>
					<!-- Basel -->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:complexType name="HerrenlosEigentumType">
		<xs:complexContent>
			<xs:extension base="grundstueck:EigentumAnteilType">
				<xs:sequence>
					<!-- In Kanton Bern gibt es keine Herrenlosgrundstücke, so keine Statistik zur Verfügung für Rechtsgründe Anzahl. -->
					<xs:element name="Rechtsgruende" type="grundstueck:RechtsgrundType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- SONDERRECHT                                                          -->
	<!-- ********************************************************** -->
	<xs:complexType name="SonderrechtType">
		<xs:sequence>
			<xs:element name="Sonderrecht" type="xs:string"/>
			<!-- 98% der Sonderrechte haben nur 1 Rechtsgrund -->
			<xs:element name="Rechtsgruende" type="grundstueck:RechtsgrundType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
		<xs:attribute name="MasterID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- STAMMGRUNDSTUECK                                           -->
	<!-- ********************************************************** -->
	<xs:complexType name="StammGrundstueckType">
		<xs:sequence>
			<xs:element name="Quote" type="grundstueck:QuoteType" minOccurs="0"/>
			<xs:element name="BelastetesGrundstueck" type="grundstueck:GrundstueckKopfType"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- DOMINIERTESGRUNDSTUECK                               -->
	<!-- ********************************************************** -->
	<xs:complexType name="DominiertesGrundstueckType">
		<xs:sequence>
			<xs:element name="BerechtigtesGrundstueckIDREF" type="xs:IDREF">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique qui lie un immeuble grevé et l'immeuble bénéficiaire.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Referenze auf eines Grundstück. Verbindet dominiertes Grundstück mit berechtigtem Grundstueck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Quote" type="grundstueck:QuoteType" minOccurs="0"/>
			<xs:element name="BelastetesGrundstueck" type="grundstueck:GrundstueckKopfType"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- FLAECHENANTEIL                                                     -->
	<!-- ********************************************************** -->
	<!-- FlaechenAnteil hat eine N zu 1 Beziehung mit Grundstück, so werden die Daten nicht dupliziert. -->
	<xs:complexType name="FlaechenAnteilType">
		<xs:sequence>
			<!-- FlaechenAnteil und SdR bezeichnen eine N:1 Beziehung. -->
			<xs:element name="SdRIDREF" type="xs:IDREF">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique sur un immeuble de droit distinct et permanent DDP. Lie la part de surface au DDP.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Referenze auf eines SelbstaendigesDauerndesRecht. Verbindet Flaechen Anteile mit SdR.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Flaeche" type="xs:decimal"/>
			<xs:element name="BelastetesGrundstueck" type="grundstueck:GrundstueckKopfType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- DIENSTBARKEIT                                                         -->
	<!-- ********************************************************** -->
	<xs:complexType name="BelegType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Le justificatif se compose normalement d'une date et d'un numéro. Dans le cas d'une affaire ancienne, le justification est identifié au moyen de l'ancier numéro de justificatif.
			Le numéro de justification n'est unique qu'avec le numéro de l'office. </xs:documentation>
			<xs:documentation xml:lang="de">Der Beleg besteht normalerweise aus einem Datum und einer Belegnummer. Handelt es sich um ein altes Geschäft wird anstelle der Belegnummer der alte Beleg angegeben. Der Beleg ist nur zusammen mit der Amtsnummer eindeutig.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!-- AmtNummer sollte nicht optional sein aber ist immer null in DB zurszeit. -->
			<xs:element name="AmtNummer" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="BelegJahr" type="xs:unsignedInt"/>
			<xs:element name="BelegNummer" type="xs:unsignedInt"/>
			<xs:element name="BelegNummerIndex" type="xs:unsignedInt" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:complexType name="DienstbarkeitZuBelastetesGrundstueckType">
		<xs:sequence>
			<xs:element name="BelastetesGrundstueck" type="grundstueck:GrundstueckKopfType"/>
			<xs:element name="Rechtsgruende" type="grundstueck:RechtsgrundType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- Meinstens Gibt es nur 1 Dienstbarkeit pro Grundstück. -->
	<xs:complexType name="DienstbarkeitType">
		<xs:sequence>
			<xs:element name="SdRIDREF" type="xs:IDREF" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique d'un immeuble de droit distinct et permanent. Lie une servitude à un DDP.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Referenze auf eines Grundstück. Verbindet Dienstbarkeiten mit SdR</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Stichwort" type="grundstueck:CapiCodeType" minOccurs="0"/>
			<xs:element name="Zusatz" type="xs:string" minOccurs="0"/>
			<xs:element name="Beleg" type="grundstueck:BelegType" minOccurs="0"/>
			<xs:element name="BelegAlt" type="xs:string" minOccurs="0"/>
			<xs:element name="BeginDatum" type="xs:date"/>
			<xs:element name="AblaufDatum" type="xs:date" minOccurs="0"/>
			<xs:element name="Entschaedigung" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="Wert" type="xs:unsignedInt" minOccurs="0"/>
			<xs:element name="FremdID" type="xs:string" minOccurs="0"/>
			<xs:element name="Meldungspflichtig" type="xs:string" minOccurs="0"/>
			<xs:element name="Personenberechtigt" type="xs:boolean" minOccurs="0"/>
			<xs:element name="Grundstueckeberechtigt" type="xs:boolean" minOccurs="0"/>
			<xs:element name="DienstbarkeitZuBelastetesGrundstueck" type="grundstueck:DienstbarkeitZuBelastetesGrundstueckType" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
		<xs:attribute name="MasterID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- QUOTEKUHRECHT                                                     -->
	<!-- ********************************************************** -->
	<xs:complexType name="QuoteKuhRechtType">
		<xs:sequence>
			<xs:element name="Ganzezahl" type="xs:unsignedInt"/>
			<xs:element name="Zaehler" type="xs:decimal" minOccurs="0"/>
			<xs:element name="Nenner" type="xs:unsignedInt" minOccurs="0"/>
			<!-- 92% der QuoteKuhRecht haben nur 1 Rechtsgrund (Total in BE ~670 Rechtsgründe für QuoteKuhRecht). -->
			<xs:element name="Rechtsgruende" type="grundstueck:RechtsgrundType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
		<xs:attribute name="MasterID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- LOKALISATION, NOMENKLATUR                            -->
	<!-- ********************************************************** -->
	<xs:simpleType name="LokalisationArtType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="benanntesGebiet"/>
			<xs:enumeration value="strasse"/>
			<xs:enumeration value="platz"/>
			<xs:enumeration value="nichtWeiterUnterschieden"/>
		</xs:restriction>
		<!-- Entspricht DB -->
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:complexType name="LokalisationType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Bezeichnungen, wei benannete Gebiete, Strassen, Plätze oder weitere</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TextDe" type="xs:string" minOccurs="0"/>
			<xs:element name="TextFr" type="xs:string" minOccurs="0"/>
			<xs:element name="TextIt" type="xs:string" minOccurs="0"/>
			<xs:element name="Art" type="grundstueck:LokalisationArtType"/>
			<xs:element name="KurzText" type="xs:string" minOccurs="0"/>
			<xs:element name="IndexText" type="xs:string" minOccurs="0"/>
			<xs:element name="Sortierbegriff" type="xs:string" minOccurs="0"/>
			<xs:element name="OffizielleBezeichnung" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:simpleType name="NomenklaturArtType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="flurname"/>
			<xs:enumeration value="ortsname"/>
			<xs:enumeration value="gelaendename"/>
		</xs:restriction>
		<!-- Entspricht DB -->
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:complexType name="NomenklaturType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Descriptions comme des noms de lieu-dts, de lieux ou de sites.</xs:documentation>
			<xs:documentation xml:lang="de">Bezeichnungen, wie Flurnamen, Ortsnamen oder Geländenamen</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Text" type="xs:string"/>
			<xs:element name="Art" type="grundstueck:NomenklaturArtType"/>
			<xs:element name="Ordnungsnummer" type="xs:string" minOccurs="0"/>
			<xs:element name="Sortierbegriff" type="xs:string" minOccurs="0"/>
			<xs:element name="DefinitiveSchreibweise" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- GRUNDSTUECKADRESSE                                      -->
	<!-- ********************************************************** -->
	<!-- Durschnitt gibt es pro Adresse 10 Grundstücke, dies bedeutet, dass Adresse Daten werden durchschnitt 10 Mal dupliziert. -->
	<!-- Geschätze Datei Grösse für den ganzen Kanton BE ist 56MB. -->
	<!-- 55% der Grundstücken haben nur eine Adresse, 0.05% der Grundstücke haben mehr als 10 Adressen. -->
	<xs:element name="GrundstueckAdresse" type="grundstueck:GrundstueckAdresseType"/>
	<xs:complexType name="GrundstueckAdresseType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Attributs d'une adresse d'immeubles, par exemple un lieu-dit.</xs:documentation>
			<xs:documentation xml:lang="de">Attribute von Grundstückadresse. Drin kann Flurname gespeichert werden.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GrundstueckIDREF" type="xs:IDREF">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique sur un immeuble. Lie l'adresse à l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Referenze auf eines Grundstück. Verbindet GrundstueckAdresse mit Grundstueck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdresseText" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">L'adresse de l'immeuble sous forme de texte.</xs:documentation>
					<xs:documentation xml:lang="de">Die Grundstückadresse in Textform.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="LokalisationGrundstueckAdresse" type="grundstueck:LokalisationGrundstueckAdresseType" substitutionGroup="grundstueck:GrundstueckAdresse"/>
	<xs:complexType name="LokalisationGrundstueckAdresseType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Une localisation, seulement pour Berne.</xs:documentation>
			<xs:documentation xml:lang="de">Eine Lokalisation Grundstückadresse, nur bei Bern.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckAdresseType">
				<xs:sequence>
					<xs:element name="Lokalisation" type="grundstueck:LokalisationType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="NomenklaturGrundstueckAdresse" type="grundstueck:NomenklaturGrundstueckAdresseType" substitutionGroup="grundstueck:GrundstueckAdresse"/>
	<xs:complexType name="NomenklaturGrundstueckAdresseType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Une nomenclature, seulement pour Berne.</xs:documentation>
			<xs:documentation xml:lang="de">Eine Nomenklatur Grundstückadresse, nur bei Bern.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="grundstueck:GrundstueckAdresseType">
				<xs:sequence>
					<xs:element name="Nomenklatur" type="grundstueck:NomenklaturType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- GBPlan                                                                          -->
	<!-- ********************************************************** -->
	<!-- Zurzeit in Kanton Bern hat ein Grundstück nur 1 GbPlan. -->
	<xs:complexType name="GbPlanType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Plan du registre foncier.</xs:documentation>
			<xs:documentation xml:lang="de">Grundbuchplan.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<!-- Elementen GbPlan und Grundstueck bezeichnen eine N:1 Beziehung. -->
			<xs:element name="GrundstueckIDREF" type="xs:IDREF">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique sur un immeuble. Lie un plan du registre foncier à l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Technische Referenze auf eines Grundstück. Verbindet GbPlan mit Grundstueck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AvKreis" type="xs:string" minOccurs="0"/>
			<xs:element name="PlanNr" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro de plan.</xs:documentation>
					<xs:documentation xml:lang="de">Plannummer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UebersichtsPlanNr" type="xs:string" minOccurs="0"/>
			<xs:element name="KatasterNr" type="xs:unsignedInt" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- GEBAEUDE                                                                  -->
	<!-- ********************************************************** -->
	<xs:simpleType name="QualitaetKoordinatenType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="av93"/>
			<xs:enumeration value="digitalisiert"/>
			<xs:enumeration value="extern"/>
			<xs:enumeration value="nicht_vorhanden"/>
		</xs:restriction>
		<!-- Entspricht DB -->
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:simpleType name="EinzelobjektArtType">
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="unterirdisches_gebaeude"/>
			<xs:enumeration value="uebriger_gebaeudeteil"/>
			<xs:enumeration value="bruecke_passerelle"/>
			<xs:enumeration value="reservoir"/>
			<xs:enumeration value="unterstand"/>
			<xs:enumeration value="aussichtsturm"/>
			<xs:enumeration value="ruine_archaeologisches_objekt"/>
		</xs:restriction>
		<!-- Entspricht DB -->
	</xs:simpleType>
	<!-- ********************************************************** -->
	<xs:complexType name="GebaeudeArtType">
		<xs:sequence>
			<xs:element name="GebaeudeArtCode" type="grundstueck:CapiCodeType" minOccurs="0"/>
			<xs:element name="GebaeudeArtZusatz" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:simpleType name="ProjektiertStatusType">
