<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:grundstueck="http://bedag.ch/capitastra/schemas/A51/v20140310/Datenexport/Grundstueck"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://bedag.ch/capitastra/schemas/A51/v20140310/Datenexport/Grundstueck"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0" xml:lang="fr">

	<xs:annotation>
		<xs:documentation xml:lang="de">Schema für Konstrukte im Zusammenhang mit Grundstücken.
		</xs:documentation>
	</xs:annotation>
	<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- Elementen                                                                                                                                             -->
	<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="GrundstueckExport">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Elément racine pour l'exportation des données.</xs:documentation>
			<xs:documentation xml:lang="de">Wurzel Element für Datenexport.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<!-- Grundstueck -->
				<xs:element name="GrundstueckList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Contient la liste des immeubles qui sont exportés.</xs:documentation>
						<xs:documentation xml:lang="de">Enthählt die Liste von Grundstücken, die exportiert werden.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice maxOccurs="unbounded">
								<xs:element name="Liegenschaft" type="grundstueck:LiegenschaftType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Immeuble de type bien-fonds</xs:documentation>
										<xs:documentation xml:lang="de">Liegenschaft.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Sdr" type="grundstueck:SDRType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Immeuble de type droit distinct et permanent DDP.</xs:documentation>
										<xs:documentation xml:lang="de">Selbständiges und Dauerndes Baurecht.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="StockwerksEinheit" type="grundstueck:StockwerksEinheitType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Propriété par étage PPE.</xs:documentation>
										<xs:documentation xml:lang="de">Stockwerkeigentum.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="GewoehnlichesMiteigentum" type="grundstueck:GewoehnlichesMiteigentumType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Part de copropriété.</xs:documentation>
										<xs:documentation xml:lang="de">Gewoenliches Miteigentum.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Folio" type="grundstueck:FolioType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Seulement pour Berne.</xs:documentation>
										<xs:documentation xml:lang="de">Nur bei Bern.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Bergwerk" type="grundstueck:BergwerkType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Mine.</xs:documentation>
										<xs:documentation xml:lang="de">Bergwerk.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="UnbekanntesGrundstueck" type="grundstueck:UnbekanntesGrundstueckType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Immeuble de type inconnu.</xs:documentation>
										<xs:documentation xml:lang="de">Art des Grundstücks ist noch nicht definiert, bekannt.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<!-- Eigentum -->
				<xs:element name="EigentumList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste des propriétaires.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Eigentum.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice maxOccurs="unbounded">
								<xs:element name="PersonEigentumAnteil" type="grundstueck:PersonEigentumAnteilType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Décrit une personne qui est propriétaire d'un immeuble et combien de cet immeuble lui appartient.</xs:documentation>
										<xs:documentation xml:lang="de">Beschreibt eine Person, die von einem Grundstück Eigentümer ist,
										und wie viel des Grundstück gehört zu der Person.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="GrundstueckEigentumAnteil" type="grundstueck:GrundstueckEigentumAnteilType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Décrit un immeuble qui est propriétaire d'un autre immeuble et combien de cet immeuble lui appartient.</xs:documentation>
										<xs:documentation xml:lang="de">Beschreibt ein Grundstück, das von einem anderen Grundstück Eigentümer ist,
										und wie viel des Grundstück gehört zu dem berechtigten Grundsück.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="HerrenlosEigentum" type="grundstueck:HerrenlosEigentumType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Décrit un immeuble sans propriétaire.</xs:documentation>
										<xs:documentation xml:lang="de">Beschreibt ein Grundstück ohne Eigetümer.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PersonstammList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste des personnes.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Personstamm.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice maxOccurs="unbounded">
								<xs:element name="NatuerlichePersonstamm" type="grundstueck:NatuerlichePersonstammType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Une personne physique.</xs:documentation>
										<xs:documentation xml:lang="de">Eine natürliche Person.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="JuristischePersonstamm" type="grundstueck:JuristischePersonstammType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Une personne juridique</xs:documentation>
										<xs:documentation xml:lang="de">Eine juristische Person.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PersonstammKantonaleIdentifikationList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Une liste de personnes, qui sont seulement représentées par leur identification cantonale (BE: Numéro GCP, VD: ACI et IROLE).</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Personstamm, die nur durch kantonale Identifikation (BE: ZPV Nummer, VD: ACI und IROLE) repraesentiert werden.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PersonstammKantonaleIdentifikation" type="grundstueck:PersonstammKantonaleIdentifikationType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Une personne, qui est seulement représentée par leur identification cantonale (BE: Numéro GCP, VD: ACI et IROLE).</xs:documentation>
									<xs:documentation xml:lang="de">Eine Personstamm, die nur durch kantonale Identifikation (BE: ZPV Nummer, VD: ACI und IROLE) repraesentiert wird.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<!-- Beschreibung -->
				<xs:element name="FlaechenAnteilList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste des parts de surface.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Flaechen Anteil.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="FlaechenAnteil" type="grundstueck:FlaechenAnteilType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Combien de la surface d'un immeuble appartien à un DDP.</xs:documentation>
									<xs:documentation xml:lang="de">Wie viele der Flaeche eines Grundstück gehört zu einem SdR.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DienstbarkeitList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Une liste de servitudes.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Dienstbarkeit.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Dienstbarkeit" type="grundstueck:DienstbarkeitType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Droit réel limité qui procure à son titulaire l'usage et/ou la jouissance d'une chose.</xs:documentation>
									<xs:documentation xml:lang="de">Beschränktes dingliches Recht, aufgrund dessen dem Berechtigten
									der Gebrauch und / oder der Genuss einer Sache zusteht.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="GrundstueckAdresseList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste des adresses des immeubles.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Grundstück Adresse.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="de">Eine Adresse eines Grundstücks.</xs:documentation>
								</xs:annotation>
								<xs:element name="GrundstueckAdresse" type="grundstueck:GrundstueckAdresseType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Une adresse d'immeuble, valable seulement pour Vaud.</xs:documentation>
										<xs:documentation xml:lang="de">Eine Grundstück Adresse, nur für Waadt.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="LokalisationGrundstueckAdresse" type="grundstueck:LokalisationGrundstueckAdresseType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Une adresse d'immeuble de type localisation, valable seulement pour Berne.</xs:documentation>
										<xs:documentation xml:lang="de">Eine Grundstück Adresse vom Typ Lokalisation, nur für Bern.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="NomenklaturGrundstueckAdresse" type="grundstueck:NomenklaturGrundstueckAdresseType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Une adresse d'immeuble de type nomenclature, valable seulement pour Berne.</xs:documentation>
										<xs:documentation xml:lang="de">Eine Grundstück Adresse vom Typ Nomenklatur, nur für Bern.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="GbPlanList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste de plans du registre foncier.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Grundbuch Pläne.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="GbPlan" type="grundstueck:GbPlanType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Complète le livre principal, basé sur un document du relevé cadastral qui contient une
									représentation géométrique de l'immeuble et en indiquant leur emplacement et limites.</xs:documentation>
									<xs:documentation xml:lang="de">Das Hauptbuch ergänzendes, auf einer amtlichen Vermessung beruhendes
									Dokument, das eine geometrische Darstellung der Grundstück enthält und deren Lage und Grenzen angibt.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="GebaeudeList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Une liste de bâtiments.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Gebaeuden.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Gebaeude" type="grundstueck:GebaeudeType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Description d'un bâtiment.</xs:documentation>
									<xs:documentation xml:lang="de">Baute.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ProjektiertGebaeudeList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste d'immeubles projetés, seulement pour Berne.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von projektierten Gebaeude, nur bei Bern.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ProjektiertGebaeude" type="grundstueck:ProjektiertGebaeudeType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Bâtiment projeté, seulement pour Berne.</xs:documentation>
									<xs:documentation xml:lang="de">Projektierte Baute, nur bei Bern.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="BodenbedeckungList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste des couvertures du sol.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von Bodenbedeckungen.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Bodenbedeckung" type="grundstueck:BodenbedeckungType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Une couverture du sol.</xs:documentation>
									<xs:documentation xml:lang="de">Eine Bodenbedeckung.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="AvBemerkungList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste de remarque de la mensuration officielle (MO).</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von amtlichen Vermessung Bemerkungen.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:choice maxOccurs="unbounded">
								<xs:element name="LageFixpunkt" type="grundstueck:LageFixpunktType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Point fixe.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Naturdenkmal" type="grundstueck:NaturdenkmalType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Site naturel.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="StreitigeGrenze" type="grundstueck:StreitigeGrenzeType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Limite contestée.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Rutschgebiet" type="grundstueck:RutschgebietType">
									<xs:annotation>
										<xs:documentation xml:lang="fr">Zone de glissement de terrain.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DominiertesGrundstueckList" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="fr">Liste de fonds servants.</xs:documentation>
						<xs:documentation xml:lang="de">Eine Liste von dominierten Grundstücke.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DominiertesGrundstueck" type="grundstueck:DominiertesGrundstueckType" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation xml:lang="fr">Décrit quel immeuble est dominé et par quel autre immeuble.</xs:documentation>
									<xs:documentation xml:lang="de">Beschreibt welches Grundstück dominiert ist und durch welches anderes Grundstück.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
            <xs:attribute name="datenschutzbereinigt" type="xs:boolean"/>
            <xs:attribute name="exportDatum" type="xs:date"/>
        </xs:complexType>
	</xs:element>
	<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- Allgemein                                                                                                                                    -->
	<!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- ********************************************************** -->
	<!-- CAPICODE                                                                   -->
	<!-- ********************************************************** -->
	<xs:complexType name="CapiCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Code selon la gestion des codes. Textes dans la langue correspondante ne sont livrés que dans la mesure où ils existent.</xs:documentation>
			<xs:documentation xml:lang="de">Codes aus der Codeverwaltung. Texte in der jeweiligen Sprache werden nur geliefert, wenn sie vorhanden sind.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TextDe" type="xs:string" minOccurs="0"/>
			<xs:element name="TextFr" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- RECHTSGRUND                                                          -->
	<!-- ********************************************************** -->
	<xs:complexType name="RechtsgrundType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Un titre de droit avec un justificatif.</xs:documentation>
			<xs:documentation xml:lang="de">Ein Rechtsgrund mit Beleg.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AmtNummer" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Numéro de l'office foncier pour ce justificatif.</xs:documentation>
					<xs:documentation xml:lang="de">Die Nummer des Grundbuchamtes für diesen Beleg.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RechtsgrundCode" type="grundstueck:CapiCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Code du titre de droit.</xs:documentation>
					<xs:documentation xml:lang="de">Der Code des Rechtsgrunds.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RechtsgrundZusatz" type="xs:string" minOccurs="0"/>
			<xs:element name="BelegDatum" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Date du justificatif.</xs:documentation>
					<xs:documentation xml:lang="de">Das Datum des Beleges.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BelegJahr" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Année du justificatif.</xs:documentation>
					<xs:documentation xml:lang="de">Das Jahr des Beleges.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BelegNummer" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Le numéro du justificatif.</xs:documentation>
					<xs:documentation xml:lang="de">Die Nummer des Beleges.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BelegNummerIndex" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">L'index au numéro de justificatif.</xs:documentation>
					<xs:documentation xml:lang="de">Der Index zur Nummer des Beleges.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BelegAlt" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Ancienne identification du justificatif.</xs:documentation>
					<xs:documentation xml:lang="de">Alte Identifikations des Beleges.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="VersionID" type="xs:string" use="optional"/>
	</xs:complexType>
	<!-- ********************************************************** -->
	<!-- EIGENTUMANTEIL                                                      -->
	<!-- ********************************************************** -->
	<xs:complexType name="QuoteType">
		<xs:annotation>
			<xs:documentation xml:lang="fr">La quote-part.</xs:documentation>
			<xs:documentation xml:lang="de">Wertquote.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AnteilZaehler" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Le numérateur de la part de l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Der Zähler des Grundstückanteils.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AnteilNenner" type="xs:unsignedLong" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Le dénominateur de la part de l'immeuble.</xs:documentation>
					<xs:documentation xml:lang="de">Der Nenner des Grundstückanteils.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QuoteInProsa" type="xs:string" minOccurs="0"/>
			<xs:element name="QuoteUnbekannt" type="xs:boolean" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ********************************************************** -->
	<xs:element name="EigentumAnteil" type="grundstueck:EigentumAnteilType"/>
	
	<xs:complexType name="EigentumAnteilType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="fr">Attributs de la propriété.</xs:documentation>
			<xs:documentation xml:lang="de">Attribute der Eigentum.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Quote" type="grundstueck:QuoteType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="fr">La cote-part.</xs:documentation>
					<xs:documentation xml:lang="de">Der Bruchteil.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BelastetesGrundstueckIDREF" type="xs:IDREF">
				<xs:annotation>
					<xs:documentation xml:lang="fr">Référence technique sur l'immeuble grevé.</xs:documentation>
