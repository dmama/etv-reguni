<?xml version="1.0"?>
<jxb:bindings version="2.1"
			  xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
			  xmlns:xs="http://www.w3.org/2001/XMLSchema"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance"
			  xmlns:namespace="http://jaxb2-commons.dev.java.net/namespace-prefix"
			  xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd
			  http://jaxb2-commons.dev.java.net/namespace-prefix http://java.net/projects/jaxb2-commons/sources/svn/content/namespace-prefix/trunk/src/main/resources/prefix-namespace-schema.xsd">

	<!-- local XSDs -->

	<jxb:bindings schemaLocation="http://ressources.etat-de-vaud.ch/fiscalite/registre/unireg-common-1.xsd">
		<jxb:schemaBindings>
			<jxb:package name="ch.vd.unireg.xml.common.v1" />
		</jxb:schemaBindings>
		<jxb:bindings node="//xs:complexType[@name='rangeType']">
			<inheritance:implements>ch.vd.unireg.xml.common.v1.DateRange</inheritance:implements>
		</jxb:bindings>
		<jxb:bindings>
			<namespace:prefix name="common-1" />
		</jxb:bindings>
	</jxb:bindings>

	<jxb:bindings schemaLocation="http://ressources.etat-de-vaud.ch/fiscalite/registre/unireg-exception-1.xsd">
		<jxb:schemaBindings>
			<jxb:package name="ch.vd.unireg.xml.exception.v1" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="ex-1" />
		</jxb:bindings>
	</jxb:bindings>

</jxb:bindings>