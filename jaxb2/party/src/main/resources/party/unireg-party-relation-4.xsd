				</jxb:property>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="relation:relationBetweenPartiesType">
				<xs:sequence>
					<xs:any minOccurs="0" maxOccurs="unbounded" processContents="lax" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

