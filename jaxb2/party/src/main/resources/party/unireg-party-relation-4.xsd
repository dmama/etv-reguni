			<xs:appinfo>
				<jxb:property>
					<jxb:javadoc>Rapport de scission d'entreprise (depuis l'entreprise scindée vers l'entreprise avant scission).</jxb:javadoc>
				</jxb:property>
			</xs:appinfo>
		</xs:annotation>
