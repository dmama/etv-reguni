<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="http://vd.ch/Editique/PM/UniregImpression-15.1"
    xmlns="http://vd.ch/Editique/PM/UniregImpression-15.1">
    <xs:annotation>
        <xs:appinfo>IMPORTANT : Le namespace par défaut ne doit pas contenir de préfixe !!!</xs:appinfo>
    </xs:annotation>
    <xs:element name="FichierImpression">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" name="infoTechnique" type="CTypeInfoTechnique"
                    nillable="false">
                    <xs:annotation>
                        <xs:documentation>(Cette partie n'est pas à renseigner par le métier, elle sera rajoutée par les Services Editique)</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element maxOccurs="unbounded" name="Document">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element minOccurs="0" name="infoDocument" type="CTypeInfoDocument"/>
                            <xs:element minOccurs="0" name="infoArchivage" type="CTypeInfoArchivage"/>
                            <xs:element minOccurs="0" name="infoEnteteDocument"
                                type="CTypeInfoEnteteDocument" nillable="false"/>
                            <xs:group ref="typeDocument" maxOccurs="1"/>
                            <xs:element name="infoRoutage" type="CTypeInfoRoutage" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation/>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:group name="typeDocument">
        <xs:choice>
            <xs:group ref="DI">
                <xs:annotation>
                    <xs:documentation>DI PM : CodDoc=U1P1, typDoc=DI</xs:documentation>
                </xs:annotation>
            </xs:group>
            <xs:group ref="refusDelai">
                <xs:annotation>
                    <xs:documentation>Refus de délai : CodDoc=U1P6, typDoc=CO</xs:documentation>
                </xs:annotation>
            </xs:group>
            <xs:group ref="accordDelai">
                <xs:annotation>
                    <xs:documentation>Accord Délai : CodDoc=U1P5, typDoc=CO</xs:documentation>
                </xs:annotation>
            </xs:group>
            <xs:group ref="sommation">
                <xs:annotation>
                    <xs:documentation>Sommation : CodDoc=U1P4, typDoc=CO</xs:documentation>
                </xs:annotation>
            </xs:group>
            <xs:group ref="accordDelaiApresSomamtion">
                <xs:annotation>
                    <xs:documentation>Accord Délai Après Sommation : CodDoc=U1P8, typDoc=CO</xs:documentation>
                </xs:annotation>
            </xs:group>
            <xs:group ref="lettreBienvenue"/>
        </xs:choice>
    </xs:group>
    <xs:group name="DI">
        <xs:sequence>
            <xs:element minOccurs="0" name="declarationImpot">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="periodeFiscale" type="xs:gYear">
                            <xs:annotation>
                                <xs:documentation>AAAA</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="codeBarreDI" type="STypeCodeBarreDI">
                            <xs:annotation>
                                <xs:documentation/>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="codeControleNIP" type="STypeCodeNIP">
                            <xs:annotation>
                                <xs:documentation>X12345</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="adresseRaisonSociale" type="CTypeAdresse"> </xs:element>
                        <xs:element name="adresseRetour" type="CTypeAdresse"> </xs:element>
                        <xs:element name="dateLimiteRetour" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" name="codeRoutage" type="STypeCodeRoutage">
                            <xs:annotation>
                                <xs:documentation>21-X

(avec X=1 pour les entreprises de catégorie « PM » dont le siège est vaudois et X=3 pour les entreprises de catégorie « PM » dont le siège est hors Vaud (hors Canton ou étranger)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="siege" type="xs:string"/>
                        <xs:element minOccurs="0" name="administrationEffective" type="xs:string"/>
                        <xs:element minOccurs="1" name="debutExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="finExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="IBAN" type="STypeNumIBAN">
                            <xs:annotation>
                                <xs:documentation>CHxx BBBB BCCC CCCC CCCC C

(avec xx : Clé, B=code banque, C=numéro de compte)</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="titulaireCompte" type="xs:string"/>
                        <xs:element minOccurs="0" name="codeFlyer" type="STypeCodeFlyer"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="refusDelai">
        <xs:sequence>
            <xs:element name="refusDelai">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="periodeFiscale" type="xs:gYear">
                            <xs:annotation>
                                <xs:documentation>AAAA</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="debutExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="finExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateEnvoiContribuable" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="accordDelai">
        <xs:sequence>
            <xs:element name="accordDelai">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="periodeFiscale" type="xs:gYear">
                            <xs:annotation>
                                <xs:documentation>AAAA</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="debutExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="finExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateEnvoiContribuable" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateAccord" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="sommation">
        <xs:sequence>
            <xs:element name="sommation">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="periodeFiscale" type="xs:gYear">
                            <xs:annotation>
                                <xs:documentation>AAAA</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateEnvoiSommation" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="codeControleNIP" type="STypeCodeNIP">
                            <xs:annotation>
                                <xs:documentation>X12345</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="accordDelaiApresSomamtion">
        <xs:sequence>
            <xs:element name="accordDelaiApresSomamtion">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="periodeFiscale" type="xs:gYear">
                            <xs:annotation>
                                <xs:documentation>AAAA</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="debutExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="1" name="finExerciceCommercial" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateEnvoiContribuable" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateEnvoiSommation" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="dateAccord" type="STypeDate">
                            <xs:annotation>
                                <xs:documentation>AAAAMMJJ</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="lettreBienvenue">
        <xs:sequence>
            <xs:element name="lettreDeBienvenue">
                <xs:complexType>
                    <xs:sequence> </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:complexType name="CTypeAdresse">
        <xs:sequence>
            <xs:element maxOccurs="6" minOccurs="1" name="adresseLigne" nillable="false"
                type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeCleRgp">
        <xs:sequence>
            <xs:element name="anneeFiscale" type="xs:gYear"/>
            <xs:element name="genreDecision" type="STypeGenreDecision"/>
            <xs:element name="genreImpot" type="CTypeGenreImpot"/>
            <xs:element name="numeroTaxation" type="STypeNumeroTaxation"/>
            <xs:element name="numeroVersion" type="STypeNumeroVersion"/>
            <xs:element name="indicateurDENC" type="STypeIndicateurDENC"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeGenreImpot">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="Impot" type="STypeImpot"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeAffranchissement">
        <xs:sequence>
            <xs:element name="zone" type="STypeZoneAffranchissement">
                <xs:annotation>
                    <xs:documentation>CH,EU,RM</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="type" type="STypeTypeAffranchissement">
                <xs:annotation>
                    <xs:documentation>A,B</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeInfoArchivage">
        <xs:sequence>
            <xs:element fixed="FOLDERS" name="nomApplication">
                <xs:annotation>
                    <xs:documentation>FOLDERS</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[A-Z]{7}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="typDossier" type="STypeDossier">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:element>
            <xs:element name="nomDossier" type="STypeNumeroContribuable">
                <xs:annotation>
                    <xs:documentation>n° de ctb formaté (avec un point comme séparateur)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="typDocument" type="STypeDocument">
                <xs:annotation>
                    <xs:documentation>Code du type de document attribué par l'Editique (XXX)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idDocument" type="STypeDocIDFolders">
                <xs:annotation>
                    <xs:documentation>Identifiant FOLDERS du document </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datTravail" type="STypeDate">
                <xs:annotation>
                    <xs:documentation>Date de travail, au format AAAAMMJJ : date d'expédition d'une correspondance</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeInfoTechnique">
        <xs:sequence>
            <xs:element name="prefixe" type="STypePrefixeTECHN">
                <xs:annotation>
                    <xs:appinfo/>
                    <xs:documentation>U1P1DIPMTECHN
U1P2DIAPTECHN
U1P3BIENTECHN
U1P4SOMMTECHN
U1P5DLAITECHN
U1P6REFUTECHN
U1P7RAPLTECHN
U1P8ADASTECHN
</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="nomFichier" type="STypeFilename">
                <xs:annotation>
                    <xs:documentation>Nom du fichier au format PMP11101_aaaammjj.xml
PMP21101_aaaammjj.xml</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sigleApplication" type="STypeSigleApplication">
                <xs:annotation>
                    <xs:documentation>sigle de l'application (XX)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="codTraitement" type="STypeCodeTraitement">
                <xs:annotation>
                    <xs:documentation>B ou D</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idUtilisateur" type="xs:string">
                <xs:annotation>
                    <xs:documentation>nom ou code de l'utilisateur</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="archive" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Si archivage prévu: true</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="returnFormat" type="STyperReturnFormat">
                <xs:annotation>
                    <xs:documentation>Format du document en sortie : AFP, PCL, PDF</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeInfoDocument">
        <xs:sequence>
            <xs:element name="prefixe" type="STypePrefixeDOCUM">
                <xs:annotation>
                    <xs:appinfo/>
                    <xs:documentation>U1P1DIPMDOCUM
U1P2DIAPDOCUM
U1P3BIENDOCUM
U1P4SOMMDOCUM
U1P5DLAIDOCUM
U1P6REFUDOCUM
U1P7RAPLDOCUM
U1P8ADASDOCUM
</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="codPorteAdresse" nillable="true"
                type="STyperCodePorteAdresse">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="2" minOccurs="0" name="txtCopieMandataire" nillable="false"
                type="xs:string">
                <xs:annotation>
                    <xs:documentation>Texte Copie Mandataire et/ou Notaire</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="idEnvoi">
                <xs:annotation>
                    <xs:documentation>Code ou identifiant de l'envoi, permet de déterminer quand on commence un nouvel envoi (cas spéciaux)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:token">
                        <xs:maxLength value="10"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="typDoc" type="STypeTypeDoc">
                <xs:annotation>
                    <xs:documentation>Exemple : DI / CO

(DI = Déclaration d'Impôt, CO = Courrier)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="codDoc" type="STypeCodeDoc">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="cleRgp" type="CTypeCleRgp"> </xs:element>
            <xs:element name="populations" type="STypePopulation">
                <xs:annotation>
                    <xs:documentation>PM</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="affranchissement" type="CTypeAffranchissement"> </xs:element>
            <xs:element minOccurs="0" name="brouillon" type="xs:boolean"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeInfoEnteteDocument">
        <xs:sequence>
            <xs:element name="expediteur" type="CTypeExpediteur"> </xs:element>
            <xs:element name="destinataire" type="CTypeDestinataire"> </xs:element>
            <xs:element minOccurs="0" name="porteAdresse" type="CTypePorteAdresse"> </xs:element>
            <xs:element minOccurs="0" name="ligReference" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Ligne de référence (e.g. concerne)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeExpediteur">
        <xs:sequence>
            <xs:element name="adresse" type="CTypeAdresse"> </xs:element>
            <xs:element name="adrMes" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>adresse messagerie</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numTelephone" type="STypeNumTelFax" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>012'345'67'89</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numFax" type="STypeNumTelFax" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>012'345'67'89</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" name="numCCP" type="STypeNumCCP" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>XX-YYYYYY-Z</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="traitePar" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Titre, initiales et nom du taxateur</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ideUti" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Visa utilisateur (valeur "ZZT" pour une taxation automatique)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="localiteExpedition" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Localité présente sur les documents sous la forme :

	Lausanne, le
	
(Seule la localité est à renseigner)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateExpedition" type="STypeDate">
                <xs:annotation>
                    <xs:documentation>AAAAMMJJ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="notreReference" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:element>
            <xs:element name="votreReference" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation/>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="numIBAN" type="STypeNumIBAN">
                <xs:annotation>
                    <xs:documentation>CHXX BBBB BCCC CCCC CCCC C

        (avec XX : Clef de contrôle de 02 à 98, 
        B = code banque, 
        C = numéro de compte)
        

</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeDestinataire">
        <xs:sequence>
            <xs:element name="adresse" type="CTypeAdresse"> </xs:element>
            <xs:element minOccurs="0" name="numRecommande" type="xs:token"/>
            <xs:element name="numContribuable" type="STypeNumeroContribuable">
                <xs:annotation>
                    <xs:documentation>xxx.yyy.zz</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="3" minOccurs="0" name="numIDE">
                <xs:annotation>
                    <xs:documentation>CHE-999.999.99C</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="STypeNumIDE">
                        <xs:pattern value="[A-Z]{3}-[0-9]{3}-[0-9]{3}-[0-9]{3}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypePorteAdresse">
        <xs:sequence>
            <xs:element name="adresse" type="CTypeAdresse"> </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CTypeInfoRoutage">
        <xs:sequence>
            <xs:element name="prefixe" type="STypePrefixeROUTE">
                <xs:annotation>
                    <xs:documentation>U1P1DIPMROUTE
U1P2DIAPROUTE
U1P3BIENROUTE
U1P4SOMMROUTE
U1P5DLAIROUTE
U1P6REFUROUTE
U1P7RAPLROUTE
U1P8ADASROUTE
</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="STypePrefixeTECHN">
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{8}TECHN"/>
            <xs:whiteSpace value="preserve"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="STypeFilename">
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z0-9]{8}_[0-9]{8}.xml"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="STypeSigleApplication">
        <xs:restriction base="xs:string">
            <xs:pattern value="[A-Z]{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="STypeCodeTraitement">
        <xs:annotation>
            <xs:documentation/>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="D"/>
            <xs:enumeration value="B"/>
            <xs:pattern value="[A-Z]"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="STyperReturnFormat">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AFP"/>
            <xs:enumeration value="PCL"/>
            <xs:enumeration value="PDF"/>
            <xs:pattern value="[A-Z]{3}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="STyperCodePorteAdresse">
        <xs:restriction base="xs:string">
            <xs:enumeration value="M"/>
            <xs:enumeration value="N"/>
            <xs:enumeration value="C"/>
            <xs:pattern value="[A-Z]"/>
            <xs:enumeration value=" "/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="STypeTypeDoc">
