<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.vd.ch/fiscalite/dperm/taopp/elementsIntegrationMetier/5" xmlns:common="http://www.vd.ch/fiscalite/dperm/common/1" targetNamespace="http://www.vd.ch/fiscalite/dperm/taopp/elementsIntegrationMetier/5" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0">
	<xs:import namespace="http://www.vd.ch/fiscalite/dperm/common/1" schemaLocation="http://resources.etat-de-vaud.ch/fiscalite/dperm/common/typeSimpleDPerm-1.xsd"/>
	<xs:element name="elementsIntegrationMetier">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="identifiantRepelec" type="common:identifiantRepelecType" minOccurs="0"/>
				<xs:element name="identifiantSupervision" type="common:identifiantSupervisionType" minOccurs="0"/>
				<xs:element name="typeDocument" type="common:typeDocumentType"/>
				<xs:element name="sousTypeDocument" type="common:sousTypeDocumentType"/>
				<xs:element name="periodeFiscale" type="common:periodeFiscaleType"/>
				<xs:element name="periodeImpositionCode" type="common:codePeriodeImpositionType" minOccurs="0"/>
				<xs:element name="numeroTiers" type="common:numeroContribuableType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Numéro de contribuable de l'individu concerné (toujours rempli pour les CSR)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="numeroContribuable" type="common:numeroContribuableType">
					<xs:annotation>
						<xs:documentation>Numéro de contribuable asujetti (surcharg ou non)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence minOccurs="0">
					<xs:annotation>
						<xs:documentation>en cas de quittance implicite</xs:documentation>
					</xs:annotation>
					<xs:element name="horodatageReceptionDocument" type="xs:dateTime"/>
					<xs:element name="numeroSequenceDemande" minOccurs="0">
						<xs:simpleType>
							<xs:restriction base="xs:positiveInteger">
								<xs:minInclusive value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
				<xs:element name="montant" type="common:montantType" minOccurs="0"/>
				<xs:element name="xmlUnitaire" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Le XML avec les données originales</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="images" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="image" type="imageType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="imageType">
		<xs:sequence>
			<xs:element name="categorie" type="common:nonEmptyTokenType"/>
			<xs:element name="identifiantRepElec" type="common:identifiantRepelecType"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
