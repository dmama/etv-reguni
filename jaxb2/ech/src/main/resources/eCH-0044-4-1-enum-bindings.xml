<?xml version="1.0"?>
<jxb:bindings version="1.0"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"

              schemaLocation="http://www.ech.ch/xmlns/eCH-0044/4/eCH-0044-4-1.xsd"
              >

    <!-- Java enumeration bindings for eCH-0044-4-1.xsd -->

    <jxb:bindings node="//xs:simpleType[@name='sexType']">
        <jxb:typesafeEnumClass name="Sex">
            <jxb:typesafeEnumMember name="MASCULIN" value="1"/>
            <jxb:typesafeEnumMember name="FEMININ" value="2"/>
            <jxb:typesafeEnumMember name="INCONNU" value="3"/>
        </jxb:typesafeEnumClass>
    </jxb:bindings>
</jxb:bindings>
