											<xs:element name="notReturnedTreatmentStatusReason" type="eVD-0004:errorType"/>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="nextResults" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="searchCriteriaType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>Critères de recherche. Voir la spécification de la recherche de personnes.</xs:documentation>
			</xs:annotation>
			<xs:element name="officialName" type="xs:string" minOccurs="0"/>
			<xs:element name="originalName" type="xs:string" minOccurs="0" />
			<xs:element name="passportName" type="xs:string" minOccurs="0" />
			<xs:element name="alliancePartnershipName" type="xs:string" minOccurs="0" />
			<xs:element name="aliasName" type="xs:string" minOccurs="0" />
			<xs:element name="otherNames" type="xs:string" minOccurs="0" />
			<xs:element name="firstNames" type="xs:string" minOccurs="0" />
			<xs:element name="dateOfBirth" type="eCH-0044:datePartiallyKnownType" minOccurs="0" />
			<xs:element name="dateOfBirthFrom" type="xs:date" minOccurs="0" />
			<xs:element name="dateOfBirthTo" type="xs:date" minOccurs="0" />
			<xs:element name="dateOfDeath" type="xs:date" minOccurs="0" />
			<xs:element name="dateOfDeathFrom" type="xs:date" minOccurs="0" />
			<xs:element name="dateOfDeathTo" type="xs:date" minOccurs="0" />
			<xs:element name="sex" type="eCH-0044:sexType" minOccurs="0"/>
			<xs:element name="nationalityStatus" type="eCH-0011:nationalityStatusType" minOccurs="0"/>
			<xs:element name="nationalityCountryId" type="xs:integer" minOccurs="0" />
			<xs:element name="town" type="xs:string" minOccurs="0" />
			<xs:element name="swissZipCode" type="xs:integer" minOccurs="0" />
			<xs:element name="municipalityId" type="xs:integer" minOccurs="0" />
			<xs:element name="typeOfReportingRelationship" type="eVD-0009:typeOfReportingRelationshipCodeType" minOccurs="0" />
			<xs:element name="residencePermit" type="eCH-0006:residencePermitType" minOccurs="0"/>
			<xs:element name="street" type="xs:string" minOccurs="0" />
			<xs:element name="searchInDeadPerson" type="xs:boolean" minOccurs="0" />
			<xs:element name="searchInCanceledPerson" type="xs:boolean" minOccurs="0" />
			<xs:element name="dataSource" minOccurs="0" >
				<xs:simpleType>
					<xs:restriction base="xs:string">
					  <xs:enumeration value="ALL"/>
					  <xs:enumeration value="FEDERAL"/>
					  <xs:enumeration value="MUNICIPAL"/>
					</xs:restriction>
  				</xs:simpleType>
 				</xs:element>
			<xs:element name="contains" type="xs:boolean" minOccurs="0" />
			<xs:element name="history" type="xs:boolean" minOccurs="0" />
			<xs:element name="maxHits" type="xs:integer" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="asynchronousSearchRequestType">
		<xs:sequence>
			<xs:element name="criteria" type="eVD-0001:searchCriteriaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Critères de recherche. Renvoie la totalité des personnes si non précisé.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="referenceDate" type="xs:date" minOccurs="0" >
				<xs:annotation>
					<xs:documentation>Date de reference pour les données des personnes. Défaut: date du jour de l'extraction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="outputHistory" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Demande l'historique des personnes. Défaut: false</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maxPersonsByResponse" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nombre maximum de personnes renvoyées par fichier</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="asynchronousSearchRequest" type="eVD-0001:asynchronousSearchRequestType"/>

	<xs:element name="asynchronousSearchResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="request" type="eVD-0001:asynchronousSearchRequestType">
					<xs:annotation>
						<xs:documentation>Critères de recherche. Renvoie la totalité des personnes si non précisé.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:sequence>
						<xs:element name="totalResponsesCount" type="xs:integer">
							<xs:annotation>
								<xs:documentation>Nombre total de fichiers renvoyés par cette recherche</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="indexOfThisResponse" type="xs:integer">
							<xs:annotation>
								<xs:documentation>Index du fichier par rapport au total</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="person" type="eVD-0001:personType" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Liste des personnes retournées par cette recherche</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:element name="error" type="eVD-0004:errorType">
						<xs:annotation>
							<xs:documentation>Type d'erreur dans la request (membre du criteria en erreur)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>
