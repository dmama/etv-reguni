<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="civilDataEventService" class="ch.vd.unireg.data.CivilDataEventServiceImpl">
		<constructor-arg>
			<list>
				<ref bean="individuConnectorCache" />
				<ref bean="entrepriseConnectorCache" />
				<ref bean="wsEventDataListenerAdapter" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="fiscalDataEventService" class="ch.vd.unireg.data.FiscalDataEventServiceImpl">
		<constructor-arg>
			<list>
				<ref bean="securityProviderCache" />
				<ref bean="wsEventDataListenerAdapter" />
			</list>
		</constructor-arg>
	</bean>

</beans>
