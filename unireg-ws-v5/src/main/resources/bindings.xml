		<jxb:bindings>
			<namespace:prefix name="ws-error-1" />
		</jxb:bindings>
	</jxb:bindings>

	<jxb:bindings schemaLocation="http://resources.etat-de-vaud.ch/fiscalite/registre/unireg-ws-ack-1.xsd">
		<jxb:schemaBindings>
			<jxb:package name="ch.vd.unireg.ws.ack.v1" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="ws-ack-1" />
		</jxb:bindings>
	</jxb:bindings>

	<jxb:bindings schemaLocation="http://resources.etat-de-vaud.ch/fiscalite/registre/unireg-ws-deadline-1.xsd">
		<jxb:schemaBindings>
			<jxb:package name="ch.vd.unireg.ws.deadline.v1" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="ws-deadline-1" />
		</jxb:bindings>
	</jxb:bindings>

	<jxb:bindings schemaLocation="http://resources.etat-de-vaud.ch/fiscalite/registre/unireg-ws-taxoffices-1.xsd">
		<jxb:schemaBindings>
