	</jxb:bindings>

	<jxb:bindings schemaLocation="http://resources.etat-de-vaud.ch/fiscalite/registre/unireg-ws-modifiedtaxpayers-1.xsd">
		<jxb:schemaBindings>
			<jxb:package name="ch.vd.unireg.ws.modifiedtaxpayers.v1" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="ws-modifiedtaxpayers-1" />
		</jxb:bindings>
	</jxb:bindings>

	<jxb:bindings schemaLocation="http://resources.etat-de-vaud.ch/fiscalite/registre/unireg-ws-debtorinfo-1.xsd">
		<jxb:schemaBindings>
			<jxb:package name="ch.vd.unireg.ws.debtorinfo.v1" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="ws-debtor-1" />
		</jxb:bindings>
	</jxb:bindings>

</jxb:bindings>