<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd">
<weblogic-ejb-jar>
	<weblogic-enterprise-bean>
		<ejb-name>ServiceFiscal</ejb-name>		
		<reference-descriptor>
			<resource-description>
				<res-ref-name>jdbc/uniregDataSource</res-ref-name>
				<jndi-name>ch.vd.uniregctb.DataSource</jndi-name>
			</resource-description>			
		</reference-descriptor>
		<enable-call-by-reference>true</enable-call-by-reference>
		<jndi-name>
			ejb/ch.vd.registre.fiscal.service.ServiceFiscal-1.9
		</jndi-name>				
	</weblogic-enterprise-bean>
	<weblogic-enterprise-bean>
		<ejb-name>RetourInformationDiMDB</ejb-name>
		<message-driven-descriptor>
			<destination-jndi-name>ch.vd.fiscalite.integration.jms.DIProviderTopic</destination-jndi-name>
			<jms-client-id>HOST-INTERFACES-1.9</jms-client-id>
		</message-driven-descriptor>
	</weblogic-enterprise-bean>
	
</weblogic-ejb-jar>
